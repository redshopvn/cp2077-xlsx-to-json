    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Will ya sit with me a while?",
              "maleVariant": "",
              "stringId": "1751653949060923392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] Mornin\u0027.",
              "maleVariant": "",
              "stringId": "1751654786579546116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So... yesterday... What was that, exactly?",
              "maleVariant": "",
              "stringId": "1751657975072669696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The beginning of something amazing.",
              "maleVariant": "",
              "stringId": "1751658319592800260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A pleasant distraction.",
              "maleVariant": "",
              "stringId": "1751658742043099136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think it was the same for you and me both...",
              "maleVariant": "I think it was the same for you and me both...",
              "stringId": "1751659209204678656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think it... I mean... I hope it was the beginning of something nice.",
              "maleVariant": "I think it... I mean, I hope it was the beginning of somethin\u0027 nice.",
              "stringId": "1751659374745468928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin\u0027?",
              "maleVariant": "",
              "stringId": "1751663244695552000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Releasing tension. Right. \u0027Course.",
              "maleVariant": "",
              "stringId": "1751663978883301376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Unless... you see things differently?",
              "maleVariant": "Unless you see things, uh... differently?",
              "stringId": "1751669740279939072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y\u0027know... yesterday, I think I\u0027d made up my mind...",
              "maleVariant": "",
              "stringId": "1751670915859464192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Actually called you \u0027cause I wanted to say goodbye.",
              "maleVariant": "",
              "stringId": "1751671777755381760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027re you trying to say?!",
              "maleVariant": "",
              "stringId": "1751673126509330436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Night City\u0027s not for everyone.",
              "maleVariant": "",
              "stringId": "1751675690588028928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Since I\u0027ve known you, Judy, all you\u0027ve been is chaotic, unstable and hotheaded.",
              "maleVariant": "",
              "stringId": "1751676894034509824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gimme your hand, V.",
              "maleVariant": "",
              "stringId": "1751688305896833024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Give hand]",
              "maleVariant": "",
              "stringId": "1763534162501812228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What for?",
              "maleVariant": "",
              "stringId": "1763534572738297856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why? What for?",
              "maleVariant": "",
              "stringId": "1763535795797344256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don\u0027t know what to say...",
              "maleVariant": "",
              "stringId": "1763536185364299780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure?",
              "maleVariant": "",
              "stringId": "1763536510573854720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s like that joke…",
              "maleVariant": "",
              "stringId": "1763536567079518208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dunno what to say... Nobody\u0027s ever done anything like this for me. Really shows trust.",
              "maleVariant": "",
              "stringId": "1763537070899314688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t say anything. Just gimme your hand.",
              "maleVariant": "",
              "stringId": "1763538702735228928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All set, congrats. Just gave you unlimited access to my pad.",
              "maleVariant": "",
              "stringId": "1763539730272600064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I-if it\u0027s what you want. Or just drop by when the urge grabs you.",
              "maleVariant": "",
              "stringId": "1763540350408835072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You sure? Isn\u0027t exactly a small thing, this...",
              "maleVariant": "You sure about this? Haven\u0027t known each other that long.",
              "stringId": "1763541263978577920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s like that joke... Know the one?",
              "maleVariant": "It\u0027s like that joke, uh... Y\u0027know the one?",
              "stringId": "1763541469163929600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wanna go there now?",
              "maleVariant": "",
              "stringId": "1763541804339150848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look, Night City\u0027s not for everyone. Grew up here, thought nothin\u0027 could surprise me...",
              "maleVariant": "Look, Night City\u0027s not for everyone. I grew up here, thought nothin\u0027 could surprise me...",
              "stringId": "1765038645287305216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027ll be the same anywhere else.",
              "maleVariant": "",
              "stringId": "1765039078659571712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People shouldn\u0027t live so cramped together, on top of each other.",
              "maleVariant": "",
              "stringId": "1765039389658824704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y\u0027know... think if you lined up all cities next to each other, they\u0027d look the same.",
              "maleVariant": "",
              "stringId": "1765040187331559424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Know why I came to Night City? \u0027Cause I thought it offered opportunities you couldn\u0027t come by anywhere else.",
              "maleVariant": "",
              "stringId": "1765040403556319232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And now?",
              "maleVariant": "",
              "stringId": "1765041234733154304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe so... Maybe all cities got allurin\u0027 faces... But stay long in one and you see what\u0027s behind it.",
              "maleVariant": "",
              "stringId": "1765043703282384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I feel you...",
              "maleVariant": "",
              "stringId": "1765043852817711104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t remember, or you want me to remind you?",
              "maleVariant": "You don\u0027t remember? Or do you want me to remind you?",
              "stringId": "1799689038386126848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know exactly what I\u0027m tryna say. What did it mean, like, to you?",
              "maleVariant": "",
              "stringId": "1799690050371018752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pleasant distraction. A chance to let loose, nice way to spend the evening...",
              "maleVariant": "",
              "stringId": "1799698894799200256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027d better get goin\u0027.",
              "maleVariant": "",
              "stringId": "1799699640026357760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh... Can be such a gonk sometimes.",
              "maleVariant": "",
              "stringId": "1799719924435476480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "City\u0027s chewed me up, an\u0027 it\u0027s spittin\u0027 me out.",
              "maleVariant": "",
              "stringId": "1799725237377130496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "M\u0027gonna miss you too, V...",
              "maleVariant": "",
              "stringId": "1799747156356849664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027d pack up my shit and bail too, if I could…",
              "maleVariant": "",
              "stringId": "1799755172879753216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But... it\u0027s just not the right time.",
              "maleVariant": "",
              "stringId": "1799755534428758016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Corps\u0027re makin\u0027 sure of that. Makin\u0027 life uniform, everywhere.",
              "maleVariant": "Corps\u0027re makin\u0027 sure of that, makin\u0027 life uniform. Everywhere.",
              "stringId": "1799760435238100992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I can\u0027t leave Night City yet.",
              "maleVariant": "",
              "stringId": "1799762395437686784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now it feels like I\u0027m standin\u0027 in quicksand. That\u0027s no way to live... for anyone.",
              "maleVariant": "Now, it feels like I\u0027m standin\u0027 in quicksand. That\u0027s no way to live... for anyone.",
              "stringId": "1799765144753627136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... I can\u0027t leave Night City, not yet. But someday – who knows.",
              "maleVariant": "",
              "stringId": "1799767162566172672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can if you want. Gonna stick around for a bit before I head back.",
              "maleVariant": "",
              "stringId": "1799810038117392384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I like you. Wanna see you lots. Way I see it, there isn\u0027t anything else to consider.",
              "maleVariant": "",
              "stringId": "1799811422556479488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ummm, no...?",
              "maleVariant": "",
              "stringId": "1799813255802871808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0022What do lesbians bring on their second date?\u0022 Moving vans. Both.",
              "maleVariant": "",
              "stringId": "1799813395892625408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah-- Oh, that one? Vintage as groaners go.",
              "maleVariant": "",
              "stringId": "1799814666280198144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait.",
              "maleVariant": "",
              "stringId": "1837495541293604868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "H-hang on... that-- that didn\u0027t come out quite right.",
              "maleVariant": "Hang on... that didn\u0027t come out quite right.",
              "stringId": "1837496417886363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya.",
              "maleVariant": "",
              "stringId": "1837496711454089216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya, Judy.",
              "maleVariant": "I\u0027ll see ya, Judy.",
              "stringId": "1837497600344547328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, bye, V. Have a nice life.",
              "maleVariant": "",
              "stringId": "1837498515004809216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meant what you said, V. Don\u0027t pretend like you can take it back.",
              "maleVariant": "",
              "stringId": "1837502173259980800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let\u0027s not make this longer than it needs to be. See ya, V.",
              "maleVariant": "",
              "stringId": "1837504886521683968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Judy... s-sorry. Thought you felt the same way.",
              "maleVariant": "Judy... sorry, I... I thought you felt the same way.",
              "stringId": "1837507113915211776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You thought one thing, I thought another. Crossed wires, it happens.",
              "maleVariant": "",
              "stringId": "1837509961579192320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa, hah... Askin\u0027 me to move in?",
              "maleVariant": "Whoa... Askin\u0027 me to move in?",
              "stringId": "1846011584026730496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mornin\u0027.",
              "maleVariant": "",
              "stringId": "1856223289511993344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure about this? Haven\u0027t known each other that long...",
              "maleVariant": "",
              "stringId": "1869373333740150784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I like you, consider you a friend. Way I see it, there\u0027s nothing else to consider.",
              "maleVariant": "",
              "stringId": "1869377518497914880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Take care, V.",
              "maleVariant": "",
              "stringId": "1869386072193720324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This really it? Last time we see each other? Didn\u0027t seem so set on leaving just a sec ago.",
              "maleVariant": "",
              "stringId": "1869386075045847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See no reason to delay. I\u0027ll stay here a couple days... then head off.",
              "maleVariant": "",
              "stringId": "1869390033227309056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s been nice knowin\u0027 ya, V.",
              "maleVariant": "",
              "stringId": "1869391729823277056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya... And hit me up on the holo sometimes.",
              "maleVariant": "",
              "stringId": "1869395868645023744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "1894067942620987392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] Later.",
              "maleVariant": "",
              "stringId": "1895239887110602752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "See ya, Judy.",
              "maleVariant": "I\u0027ll see ya, Judy.",
              "stringId": "1895239889526521856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here\u0027s your coffee. Finally.",
              "maleVariant": "",
              "stringId": "1915739148995223552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re crazy. But I\u0027ll miss ya.",
              "maleVariant": "",
              "stringId": "1917219553321639936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna miss ya.",
              "maleVariant": "I\u0027m gonna miss ya.",
              "stringId": "1917262112001581056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ruined my plans, you know that?",
              "maleVariant": "",
              "stringId": "1918361810695315456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Was gonna leave Night City in the dust, for good. Even started packing.",
              "maleVariant": "",
              "stringId": "1918376736511975424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why did you wanna leave?",
              "maleVariant": "",
              "stringId": "1918384194991120388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why\u0027d you wanna leave?",
              "maleVariant": "",
              "stringId": "1918384482703597568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, well, guess I\u0027ll have to make it up to you, won\u0027t I?",
              "maleVariant": "Oh, well, uh... I guess I\u0027ll have to make it up to ya, won\u0027t I?",
              "stringId": "1918384511778512896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Positively burst into my life, you did, so I should\u0027ve expected an ending just as abrupt, like this...",
              "maleVariant": "Positively burst into my life, you did... so I should\u0027ve expected an ending just as abrupt, like this...",
              "stringId": "1918389346535956480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna make it up to you, somehow.",
              "maleVariant": "",
              "stringId": "1918396618234355712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But now I think... Think I gotta stay.",
              "maleVariant": "",
              "stringId": "1918404726344081408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y\u0027know, everything that happened... Guess it just got to me.",
              "maleVariant": "",
              "stringId": "1918406390945898496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But now, from where I stand... Night City\u0027s started to feel like the place to be.",
              "maleVariant": "",
              "stringId": "1918410556913180672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm... Yeah, that would be appropriate.",
              "maleVariant": "",
              "stringId": "1918415779878625280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The fuck, Judy? Sayin\u0027 you wanna leave? Now?",
              "maleVariant": "The fuck, Judy? You sayin\u0027 you wanna leave? Now?",
              "stringId": "1924289750914260992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Y\u0027know, had this bizarre convo with a doll... Told me I had to fight. And I thought – fight for what?",
              "maleVariant": "Y\u0027know, I had this bizarre convo with a doll... Told me I had to fight. And I thought – fight for what?",
              "stringId": "1924290877487538176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s not like we won\u0027t keep in touch...",
              "maleVariant": "",
              "stringId": "1924291545237512192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And now, just when I thought I knew what it was all for... Friendship, trust... You\u0027re tellin\u0027 me you\u0027re leavin\u0027?",
              "maleVariant": "And now, just when I thought I knew what it was all for... Friendship, trust... You\u0027re tellin\u0027 me you\u0027re leavin\u0027.",
              "stringId": "1924291792433012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pick up clothes]",
              "maleVariant": "",
              "stringId": "2240452560525975556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Pick up clothes]",
              "maleVariant": "",
              "stringId": "2240460289269293056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Drink]",
              "maleVariant": "",
              "stringId": "2260589178882424836"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}