    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You there! How \u0027bout a quick game!",
              "maleVariant": "",
              "stringId": "3153572377098899456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Another time.",
              "maleVariant": "",
              "stringId": "3153577460914114564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, some other time, maybe. Name’s V. Heard you got a job for me.",
              "maleVariant": "",
              "stringId": "3153634052594388992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell me more \u0027bout Eric. Mentioned he sells two-wheelers?",
              "maleVariant": "",
              "stringId": "3153690529401065472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You testified \u0027gainst your own son? What kind o\u0027 father does that?",
              "maleVariant": "",
              "stringId": "3153690864358182912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fixes \u0027em. Little go-getter owns a repair shop in Heywood now.",
              "maleVariant": "",
              "stringId": "3153902301437415424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V!",
              "maleVariant": "",
              "stringId": "3153912016535998464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s done. I deleted the testimony.",
              "maleVariant": "",
              "stringId": "3153912166306205700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Phew, that\u0027s a weight off my shoulders!",
              "maleVariant": "",
              "stringId": "3153919402655010816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spoke to the D.A. Scrubbed your name and data from your testimony.",
              "maleVariant": "",
              "stringId": "3153919493520412676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027ll take my payment now.",
              "maleVariant": "",
              "stringId": "3153920184339058688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Pay up.",
              "maleVariant": "",
              "stringId": "3153920184389390336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wasn\u0027t no biggie. I\u0027ll take my payment now.",
              "maleVariant": "",
              "stringId": "3153920184439721988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s done. Testimony\u0027s gone for good.",
              "maleVariant": "",
              "stringId": "3153926824274612224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gonna pay me with Georgina’s money?",
              "maleVariant": "",
              "stringId": "3153928268994236416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go on, pay me with the eddies you got for sellin\u0027 your son down the river.",
              "maleVariant": "",
              "stringId": "3153929111059484676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I... Ah, shit.",
              "maleVariant": "",
              "stringId": "3153935414460862464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Georgina scrubbed your name from the testimony.",
              "maleVariant": "",
              "stringId": "3153937132313563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait, what?! That\u0027s not what-- She was s\u0027posed to delete it!",
              "maleVariant": "",
              "stringId": "3153941667178270720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The money\u0027s Georgina\u0027s - not yours.",
              "maleVariant": "",
              "stringId": "3153943897105158144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We both know not an enny of that scratch is yours, Mikey.",
              "maleVariant": "",
              "stringId": "3153944217281548288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shut up and pay. Tired of just lookin\u0027 at ya.",
              "maleVariant": "",
              "stringId": "3154966676944375808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hands sent me.",
              "maleVariant": "",
              "stringId": "3275648391047467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Michael Maldonado? I’m V. Hands said you needed a... a hand with somethin\u0027...",
              "maleVariant": "Michael Maldonado? I’m V. Hands said you needed a, uh... well, hand with somethin\u0027...",
              "stringId": "3275649799175983104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It’s about my son, Eric. Boy runs a motorcycle repair shop in Heywood. Totally legit operation. Taxes, permits, you name it.",
              "maleVariant": "",
              "stringId": "3294292714439659520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who\u0027s this D.A.?",
              "maleVariant": "",
              "stringId": "3294297029069852672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First thing\u0027s first. This D.A. Zembinsky. what\u0027s her deal?",
              "maleVariant": "",
              "stringId": "3294300910143229952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then, they put me in front of a camera and I... I told \u0027em what wanted to hear. I was fucking scared.",
              "maleVariant": "",
              "stringId": "3294301256106201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You testified against your own son?",
              "maleVariant": "",
              "stringId": "3294323757389533184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She forced you? How?",
              "maleVariant": "",
              "stringId": "3294324550482087936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She\u0027s as crooked as they come, like everyone else in Night City.",
              "maleVariant": "",
              "stringId": "3295478827237855232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It ain\u0027t like you think, OK? I... I really didn\u0027t want to.",
              "maleVariant": "",
              "stringId": "3295516168035848192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zembinsky forced you to testify? How exactly?",
              "maleVariant": "",
              "stringId": "3295561605367287808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027ll see what I can do.",
              "maleVariant": "",
              "stringId": "3295598785070452740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not if I have anythin\u0027 to say about it. Gonna find her, have ourselves a little chat.",
              "maleVariant": "Not if I\u0027ve got anythin\u0027 to say about it. Gonna find her, have ourselves a little chat.",
              "stringId": "3295599208242171904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, that\u0027s a relief. Knew Hands wouldn\u0027t toss me some bush-leaguer.",
              "maleVariant": "",
              "stringId": "3295599796736577536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just... go talk to her – y\u0027know, the way you people do. Eric\u0027s a good kid. He\u0027ll be eaten alive in prison.",
              "maleVariant": "",
              "stringId": "3295600646620008448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She showed me her so-called \u0022evidence\u0022 \u0027gainst Eric. Wanted me to corroborate it. Was bullshit – every last word.",
              "maleVariant": "",
              "stringId": "3295602915319042048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027ll chat up Zembinsky when I get a spare sec.",
              "maleVariant": "",
              "stringId": "3295610515230117888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I’ll take care of it when I have time.",
              "maleVariant": "",
              "stringId": "3295635255550365696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t take too long. Eric’s trial\u0027s right around the corner. If you don\u0027t bend \u0027er ear, she\u0027ll lock \u0027im up.",
              "maleVariant": "",
              "stringId": "3295643765860290560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How\u0027d it go? \u0027M I finally gonna get a good night\u0027s sleep?",
              "maleVariant": "",
              "stringId": "3305600426473508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Count yourself lucky you\u0027ve only got me to deal with, Mikey.",
              "maleVariant": "",
              "stringId": "3305620921117921280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right, right, of course. Here.",
              "maleVariant": "",
              "stringId": "3305637524236750848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Forget about your payment! This wasn\u0027t part of the deal!",
              "maleVariant": "",
              "stringId": "3307015151606386688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Mikey! This piece of old scrap ate my eddies again!",
              "maleVariant": "",
              "stringId": "3323111921338822656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Glad you could make it. Fixer said you can turn around even the most hopeless of cases.",
              "maleVariant": "",
              "stringId": "3323207358281502720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How hopeless we talkin\u0027?",
              "maleVariant": "",
              "stringId": "3323212333581684736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah... moved outta Dogtown with his mother years ago. Barely seen the kid since.",
              "maleVariant": "",
              "stringId": "3323242673163554816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You’ll find her at the Heavy Hearts club. Perfect place for plottin\u0027 an\u0027 schemin\u0027.",
              "maleVariant": "",
              "stringId": "3323247108119941120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you know that how?",
              "maleVariant": "",
              "stringId": "3323270454924627968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That’s where she invited me for our... heart to heart.",
              "maleVariant": "",
              "stringId": "3323270575737360384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Two days later, came back with some badge. Beat the shit outta me, threatened worse for Eric.",
              "maleVariant": "",
              "stringId": "3323271580994588672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But this D.A., Georgina Zembinsky – she needs her pound of flesh, wants to turn \u0027im into an example. That\u0027s where you come in.",
              "maleVariant": "",
              "stringId": "3324270150774247424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just like that? For no reason?",
              "maleVariant": "",
              "stringId": "3324274428561674240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why would she have it in for him? S\u0027gotta be a reason.",
              "maleVariant": "",
              "stringId": "3324275490576224256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She\u0027s the D.A. If she wants a reason, she\u0027ll find one.",
              "maleVariant": "",
              "stringId": "3324277324191715328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now I need you to make it... go away.",
              "maleVariant": "",
              "stringId": "3324284677242834944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eric? He\u0027s too innocent for this town. \u0027S why they\u0027re fixin\u0027 to take \u0027im down.",
              "maleVariant": "",
              "stringId": "3324285618931191808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell me more about Eric.",
              "maleVariant": "",
              "stringId": "3324291273020755968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure your son\u0027s who he says he is?",
              "maleVariant": "",
              "stringId": "3324296194751287296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "At first she seemed... nice. We were sittin\u0027, havin\u0027 a normal conversation.",
              "maleVariant": "",
              "stringId": "3324300387746664448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, it’s you. Over here, let\u0027s talk.",
              "maleVariant": "",
              "stringId": "3347603068774862848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can\u0027t play like this, man. When you finally gon\u0027 fix it?",
              "maleVariant": "",
              "stringId": "3347606033023717376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did just yesterday, but... Ah, my bad. Next game’s on me, OK?",
              "maleVariant": "",
              "stringId": "3347611867283804160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Zembinsky said she’s after some Valentino kingpin... Hector Sacristán was the name, I think.",
              "maleVariant": "",
              "stringId": "3347854424152559616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What’s Eric got to do with it?",
              "maleVariant": "",
              "stringId": "3347861732307849216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Word is, he knows Sacristán, has heaps o\u0027 dirt that could bring \u0027im down. But for that, she needs Eric \u0027hind bars. So... she forced me to testify.",
              "maleVariant": "",
              "stringId": "3347861908133072896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just... just go.",
              "maleVariant": "",
              "stringId": "3368225478566543360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Much obliged.",
              "maleVariant": "",
              "stringId": "3369592540785627136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, thank you. For helping me out of a tight spot... but most of all, my son.",
              "maleVariant": "",
              "stringId": "3369592968302645248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wish I could believe that...",
              "maleVariant": "",
              "stringId": "3369593606407278592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh no, you\u0027ll pay. Job\u0027s done – your son\u0027ll never find out who squealed on \u0027im.",
              "maleVariant": "Oh no, you\u0027ll pay. Job\u0027s done. \u0027Sides, your son\u0027ll never find out who squealed on \u0027im.",
              "stringId": "3369606609437544448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So? What did that bitch say?",
              "maleVariant": "",
              "stringId": "3369607447241379840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everything and more. But don’t fret – your secret’s safe with me.",
              "maleVariant": "",
              "stringId": "3369610129096134656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "My silence, on the other hand, doesn\u0027t come cheap.",
              "maleVariant": "",
              "stringId": "3369610504100466688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I--",
              "maleVariant": "",
              "stringId": "3369610888550371328"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}