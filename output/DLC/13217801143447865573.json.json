    "Data": {
        "Version": 195,
        "BuildVersion": 0,
        "RootChunk": {
            "$type": "JsonResource",
            "cookingPlatform": "PLATFORM_PC",
            "root": {
                "HandleId": "0",
                "Data": {
                    "$type": "localizationPersistenceSubtitleEntries",
                    "entries": [
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You there! How 'bout a quick game!",
                            "maleVariant": "",
                            "stringId": "3153572377098899456"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Another time.",
                            "maleVariant": "",
                            "stringId": "3153577460914114564"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Uh, some other time, maybe. Name’s V. Heard you got a job for me.",
                            "maleVariant": "",
                            "stringId": "3153634052594388992"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Tell me more 'bout Eric. Mentioned he sells two-wheelers?",
                            "maleVariant": "",
                            "stringId": "3153690529401065472"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You testified 'gainst your own son? What kind o' father does that?",
                            "maleVariant": "",
                            "stringId": "3153690864358182912"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fixes 'em. Little go-getter owns a repair shop in Heywood now.",
                            "maleVariant": "",
                            "stringId": "3153902301437415424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "V!",
                            "maleVariant": "",
                            "stringId": "3153912016535998464"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It's done. I deleted the testimony.",
                            "maleVariant": "",
                            "stringId": "3153912166306205700"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Phew, that's a weight off my shoulders!",
                            "maleVariant": "",
                            "stringId": "3153919402655010816"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Spoke to the D.A. Scrubbed your name and data from your testimony.",
                            "maleVariant": "",
                            "stringId": "3153919493520412676"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'll take my payment now.",
                            "maleVariant": "",
                            "stringId": "3153920184339058688"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Pay up.",
                            "maleVariant": "",
                            "stringId": "3153920184389390336"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wasn't no biggie. I'll take my payment now.",
                            "maleVariant": "",
                            "stringId": "3153920184439721988"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It's done. Testimony's gone for good.",
                            "maleVariant": "",
                            "stringId": "3153926824274612224"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gonna pay me with Georgina’s money?",
                            "maleVariant": "",
                            "stringId": "3153928268994236416"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Go on, pay me with the eddies you got for sellin' your son down the river.",
                            "maleVariant": "",
                            "stringId": "3153929111059484676"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I... Ah, shit.",
                            "maleVariant": "",
                            "stringId": "3153935414460862464"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Georgina scrubbed your name from the testimony.",
                            "maleVariant": "",
                            "stringId": "3153937132313563136"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wait, what?! That's not what-- She was s'posed to delete it!",
                            "maleVariant": "",
                            "stringId": "3153941667178270720"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The money's Georgina's - not yours.",
                            "maleVariant": "",
                            "stringId": "3153943897105158144"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We both know not an enny of that scratch is yours, Mikey.",
                            "maleVariant": "",
                            "stringId": "3153944217281548288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Shut up and pay. Tired of just lookin' at ya.",
                            "maleVariant": "",
                            "stringId": "3154966676944375808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hands sent me.",
                            "maleVariant": "",
                            "stringId": "3275648391047467008"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Michael Maldonado? I’m V. Hands said you needed a... a hand with somethin'...",
                            "maleVariant": "Michael Maldonado? I’m V. Hands said you needed a, uh... well, hand with somethin'...",
                            "stringId": "3275649799175983104"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It’s about my son, Eric. Boy runs a motorcycle repair shop in Heywood. Totally legit operation. Taxes, permits, you name it.",
                            "maleVariant": "",
                            "stringId": "3294292714439659520"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Who's this D.A.?",
                            "maleVariant": "",
                            "stringId": "3294297029069852672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "First thing's first. This D.A. Zembinsky. what's her deal?",
                            "maleVariant": "",
                            "stringId": "3294300910143229952"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Then, they put me in front of a camera and I... I told 'em what wanted to hear. I was fucking scared.",
                            "maleVariant": "",
                            "stringId": "3294301256106201088"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You testified against your own son?",
                            "maleVariant": "",
                            "stringId": "3294323757389533184"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "She forced you? How?",
                            "maleVariant": "",
                            "stringId": "3294324550482087936"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "She's as crooked as they come, like everyone else in Night City.",
                            "maleVariant": "",
                            "stringId": "3295478827237855232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It ain't like you think, OK? I... I really didn't want to.",
                            "maleVariant": "",
                            "stringId": "3295516168035848192"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Zembinsky forced you to testify? How exactly?",
                            "maleVariant": "",
                            "stringId": "3295561605367287808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'll see what I can do.",
                            "maleVariant": "",
                            "stringId": "3295598785070452740"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Not if I have anythin' to say about it. Gonna find her, have ourselves a little chat.",
                            "maleVariant": "Not if I've got anythin' to say about it. Gonna find her, have ourselves a little chat.",
                            "stringId": "3295599208242171904"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Ah, that's a relief. Knew Hands wouldn't toss me some bush-leaguer.",
                            "maleVariant": "",
                            "stringId": "3295599796736577536"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Just... go talk to her – y'know, the way you people do. Eric's a good kid. He'll be eaten alive in prison.",
                            "maleVariant": "",
                            "stringId": "3295600646620008448"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "She showed me her so-called \"evidence\" 'gainst Eric. Wanted me to corroborate it. Was bullshit – every last word.",
                            "maleVariant": "",
                            "stringId": "3295602915319042048"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'll chat up Zembinsky when I get a spare sec.",
                            "maleVariant": "",
                            "stringId": "3295610515230117888"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I’ll take care of it when I have time.",
                            "maleVariant": "",
                            "stringId": "3295635255550365696"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Don't take too long. Eric’s trial's right around the corner. If you don't bend 'er ear, she'll lock 'im up.",
                            "maleVariant": "",
                            "stringId": "3295643765860290560"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How'd it go? 'M I finally gonna get a good night's sleep?",
                            "maleVariant": "",
                            "stringId": "3305600426473508864"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Count yourself lucky you've only got me to deal with, Mikey.",
                            "maleVariant": "",
                            "stringId": "3305620921117921280"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Right, right, of course. Here.",
                            "maleVariant": "",
                            "stringId": "3305637524236750848"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Forget about your payment! This wasn't part of the deal!",
                            "maleVariant": "",
                            "stringId": "3307015151606386688"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hey, Mikey! This piece of old scrap ate my eddies again!",
                            "maleVariant": "",
                            "stringId": "3323111921338822656"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Glad you could make it. Fixer said you can turn around even the most hopeless of cases.",
                            "maleVariant": "",
                            "stringId": "3323207358281502720"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How hopeless we talkin'?",
                            "maleVariant": "",
                            "stringId": "3323212333581684736"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Yeah... moved outta Dogtown with his mother years ago. Barely seen the kid since.",
                            "maleVariant": "",
                            "stringId": "3323242673163554816"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You’ll find her at the Heavy Hearts club. Perfect place for plottin' an' schemin'.",
                            "maleVariant": "",
                            "stringId": "3323247108119941120"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "And you know that how?",
                            "maleVariant": "",
                            "stringId": "3323270454924627968"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That’s where she invited me for our... heart to heart.",
                            "maleVariant": "",
                            "stringId": "3323270575737360384"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Two days later, came back with some badge. Beat the shit outta me, threatened worse for Eric.",
                            "maleVariant": "",
                            "stringId": "3323271580994588672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "But this D.A., Georgina Zembinsky – she needs her pound of flesh, wants to turn 'im into an example. That's where you come in.",
                            "maleVariant": "",
                            "stringId": "3324270150774247424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Just like that? For no reason?",
                            "maleVariant": "",
                            "stringId": "3324274428561674240"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Why would she have it in for him? S'gotta be a reason.",
                            "maleVariant": "",
                            "stringId": "3324275490576224256"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "She's the D.A. If she wants a reason, she'll find one.",
                            "maleVariant": "",
                            "stringId": "3324277324191715328"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Now I need you to make it... go away.",
                            "maleVariant": "",
                            "stringId": "3324284677242834944"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Eric? He's too innocent for this town. 'S why they're fixin' to take 'im down.",
                            "maleVariant": "",
                            "stringId": "3324285618931191808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Tell me more about Eric.",
                            "maleVariant": "",
                            "stringId": "3324291273020755968"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sure your son's who he says he is?",
                            "maleVariant": "",
                            "stringId": "3324296194751287296"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "At first she seemed... nice. We were sittin', havin' a normal conversation.",
                            "maleVariant": "",
                            "stringId": "3324300387746664448"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Oh, it’s you. Over here, let's talk.",
                            "maleVariant": "",
                            "stringId": "3347603068774862848"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Can't play like this, man. When you finally gon' fix it?",
                            "maleVariant": "",
                            "stringId": "3347606033023717376"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Did just yesterday, but... Ah, my bad. Next game’s on me, OK?",
                            "maleVariant": "",
                            "stringId": "3347611867283804160"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Zembinsky said she’s after some Valentino kingpin... Hector Sacristán was the name, I think.",
                            "maleVariant": "",
                            "stringId": "3347854424152559616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What’s Eric got to do with it?",
                            "maleVariant": "",
                            "stringId": "3347861732307849216"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Word is, he knows Sacristán, has heaps o' dirt that could bring 'im down. But for that, she needs Eric 'hind bars. So... she forced me to testify.",
                            "maleVariant": "",
                            "stringId": "3347861908133072896"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Just... just go.",
                            "maleVariant": "",
                            "stringId": "3368225478566543360"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Much obliged.",
                            "maleVariant": "",
                            "stringId": "3369592540785627136"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No, thank you. For helping me out of a tight spot... but most of all, my son.",
                            "maleVariant": "",
                            "stringId": "3369592968302645248"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wish I could believe that...",
                            "maleVariant": "",
                            "stringId": "3369593606407278592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Oh no, you'll pay. Job's done – your son'll never find out who squealed on 'im.",
                            "maleVariant": "Oh no, you'll pay. Job's done. 'Sides, your son'll never find out who squealed on 'im.",
                            "stringId": "3369606609437544448"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "So? What did that bitch say?",
                            "maleVariant": "",
                            "stringId": "3369607447241379840"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Everything and more. But don’t fret – your secret’s safe with me.",
                            "maleVariant": "",
                            "stringId": "3369610129096134656"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "My silence, on the other hand, doesn't come cheap.",
                            "maleVariant": "",
                            "stringId": "3369610504100466688"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I--",
                            "maleVariant": "",
                            "stringId": "3369610888550371328"
                        }
                    ]
                }
            }
        },
        "EmbeddedFiles": []
    }
}