    "Data": {
        "Version": 195,
        "BuildVersion": 0,
        "RootChunk": {
            "$type": "JsonResource",
            "cookingPlatform": "PLATFORM_PC",
            "root": {
                "HandleId": "0",
                "Data": {
                    "$type": "localizationPersistenceSubtitleEntries",
                    "entries": [
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention all Dogtown residents! This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "2584133210136862720"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "An unmarked aircraft has crashed in our district.",
                            "maleVariant": "",
                            "stringId": "3065195960630448128"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The crash site is now a restricted area.",
                            "maleVariant": "",
                            "stringId": "3065202296579624960"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "My people will proceed to secure it. The rest of you – go home and stay there.",
                            "maleVariant": "",
                            "stringId": "3065203029962067968"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'm imposing a state of emergency in Dogtown until further notice.",
                            "maleVariant": "",
                            "stringId": "3065215056591917056"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "More news to come as the situation develops.",
                            "maleVariant": "",
                            "stringId": "3336323385528786944"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That is, unless you wanna catch a stray bullet.",
                            "maleVariant": "",
                            "stringId": "3336327947639693312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The cyberpsycho who barged into Dogtown without registering – his bounty's now doubled.",
                            "maleVariant": "",
                            "stringId": "3365151643775197184"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Good news. Yesterday we completed some necessary repairs at the hospital.",
                            "maleVariant": "",
                            "stringId": "3365265242204069892"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Scavenger representatives will be arriving Tuesday on business.",
                            "maleVariant": "",
                            "stringId": "3365265768320786440"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Good news, Dogtown. A certain president has turned up in Washington, safe and sound.",
                            "maleVariant": "",
                            "stringId": "3365266578106671112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Here's the score: soon, it'll be seven years since we brought forth our new home here.",
                            "maleVariant": "",
                            "stringId": "3365266578157002760"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hello, Dogtown. This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3365301507028869128"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention all Dogtown residents. This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3365301866212286464"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention! This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3365301866229063688"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Over and out.",
                            "maleVariant": "",
                            "stringId": "3365305042927804424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That'll be all.",
                            "maleVariant": "",
                            "stringId": "3365305042944581640"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "More news to come shortly.",
                            "maleVariant": "",
                            "stringId": "3365305042978136072"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We've got some important guests in Dogtown, here by my personal invitation. Stay out of their way.",
                            "maleVariant": "",
                            "stringId": "3366315694819565576"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention, Dogtown! This is Bennett.",
                            "maleVariant": "",
                            "stringId": "3366340273239678984"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention Dogtown! Lieuten-- COLONEL Bennett speaking!",
                            "maleVariant": "",
                            "stringId": "3366340830276804616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "In case you haven't heard, Dogtown has a special guest. Rosalind Myers, as she lives and breathes.",
                            "maleVariant": "",
                            "stringId": "3366340957263552520"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hello, Dogtown. This is Jago.",
                            "maleVariant": "",
                            "stringId": "3366393203862986760"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Colonel Kurt Hansen is dead. I repeat: Kurt Hansen is dead.",
                            "maleVariant": "",
                            "stringId": "3394251262425145344"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It's a dark day here in Dogtown.",
                            "maleVariant": "",
                            "stringId": "3394251843554353152"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Kurt Hansen may be dead, but BARGHEST is alive and well!",
                            "maleVariant": "",
                            "stringId": "3394252636495912960"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How the fuck d'you turn this thing off...?",
                            "maleVariant": "",
                            "stringId": "3394255686862217216"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "His description: a big-ass motherfucker with a full faceplate.",
                            "maleVariant": "",
                            "stringId": "3395367408528367616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Armed, dangerous, unresponsive to any attempts at contact. Fire without warning.",
                            "maleVariant": "",
                            "stringId": "3395367608764440576"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The NUSA says the president managed to crash her Space Force One into our humble district.",
                            "maleVariant": "",
                            "stringId": "3395370272298475520"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "If that's true, well, we've got to find her. For her own safety.",
                            "maleVariant": "",
                            "stringId": "3395371142046130176"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Any information on Myers I will personally reward. In neurostims.",
                            "maleVariant": "",
                            "stringId": "3395372270968217600"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We've increased our presence on the streets and at the border checkpoints. Nobody goes in or out without a permit.",
                            "maleVariant": "",
                            "stringId": "3395372672514105344"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The Colonel went out the way he lived – gun in hand. And the motherfuckers who took him out? Oh, they'll pay. You know why?",
                            "maleVariant": "",
                            "stringId": "3395377432948482048"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Two months of hard work have finally paid off. Starting today, the hospital and its staff will be operating under our protection.",
                            "maleVariant": "",
                            "stringId": "3395476523682471936"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You need stitches? A new leg? Report directly to Dr. Robertson.",
                            "maleVariant": "",
                            "stringId": "3395477468424282112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Our Balkan-based friends might have a little jetlag, so I want everyone in their homes by six sharp.",
                            "maleVariant": "",
                            "stringId": "3395479029074468864"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Blinds down, no scrolling.",
                            "maleVariant": "",
                            "stringId": "3395479345962524672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Over the years our endurance was tested – by force, by coercion, by empty promises. But we endured. And hell, I'm moved to reminisce.",
                            "maleVariant": "",
                            "stringId": "3395481131528069120"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "These days, you could light a smoke and spit the butt over to the other side. But I tell you what – seven years ago, that channel was broader and you didn't spit butts, you spat lead.",
                            "maleVariant": "",
                            "stringId": "3395481407664267264"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I received three orders during our attack on the city. The first – take the peninsula south of the storm channel. We did. The second – set up bases, hold 'em. We held.",
                            "maleVariant": "",
                            "stringId": "3395481667140689920"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The third – withdraw. Fuck your positions. Well, that was the final order. You know why? Look at that storm channel. I see you sippin' beer there by day, collecting cargo by night. Shit's downright bucolic.",
                            "maleVariant": "",
                            "stringId": "3395481855599157248"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Let me tell you about Stan. Stan was a fine Southern boy. Dreamed of a career in dance, wound up in our assault force. Life doesn't always give you a choice.",
                            "maleVariant": "",
                            "stringId": "3395482398040104960"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That night, Stan was runnin' in front of me. When he suddenly spun round like he was doin' a pirouette, I thought: fuckin' Christ Stan, not now! And then I saw the enemy snipers.",
                            "maleVariant": "",
                            "stringId": "3395482621512622080"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Joey – mechanic from Atlanta, two kids. Emily – engineer with a heart of gold. Carl – paramedic, no heart and a weird thirst for blood, but hell, you'd still like the guy.",
                            "maleVariant": "",
                            "stringId": "3395483304009768960"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I won't give you their ranks. I'd rather remember their names. Too many names to count...",
                            "maleVariant": "",
                            "stringId": "3395483381117853696"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We did it. Everything we were ordered to do. And then the brass decided to flush us down the storm drain. YOU'RE GONE – I heard – WERE NEVER THERE. And I'd had enough of the lies.",
                            "maleVariant": "",
                            "stringId": "3395483935504179200"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "A moment of silence for Stan and the rest? ...No. I got a better idea. Grab your iron, whatever you got. Go out on the street and give 'em a thousand-gun salute.",
                            "maleVariant": "",
                            "stringId": "3395484184578727936"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "They earned it. Let 'em know up in high heaven that Dogtown's still kickin'. Let 'em know we're still here – and fuck if we're goin' anywhere else!",
                            "maleVariant": "",
                            "stringId": "3395484920645525504"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Still no news of our missing Madam President.",
                            "maleVariant": "",
                            "stringId": "3395522225674493956"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "For those who don't know me – I've replaced Colonel Kurt Hansen. May he rest in peace.",
                            "maleVariant": "",
                            "stringId": "3395578197990285312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We're introducing some organizational changes, but trust your business interests in the district remain secure.",
                            "maleVariant": "",
                            "stringId": "3395579945001787392"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'll do my best to fill the shoes of the one man our town, Dogtown, owes everything to.",
                            "maleVariant": "",
                            "stringId": "3395581467500584960"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This will be the final radio announcement. We are working on a more... contemporary means of communication.",
                            "maleVariant": "",
                            "stringId": "3395583349920997376"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Over?",
                            "maleVariant": "",
                            "stringId": "3395592548902084608"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It's official. Starting today, I'm in charge of BARGHEST.",
                            "maleVariant": "",
                            "stringId": "3395595323232854016"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You residents of Dogtown can look forward to a bright future full of uh... bright days.",
                            "maleVariant": "",
                            "stringId": "3395608639023980544"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Meanin' residents who know how to follow orders, of course.",
                            "maleVariant": "",
                            "stringId": "3395611115072626688"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That's all for now. Over and out.",
                            "maleVariant": "",
                            "stringId": "3395612340312711168"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I have to say, I'm a little... disappointed.",
                            "maleVariant": "",
                            "stringId": "3395620918973014016"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Reward's doubled for any intel on Rosalind Myers' whereabouts. Over and out.",
                            "maleVariant": "",
                            "stringId": "3395621197927784448"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Now, if I were a betting man, I'd say she was there all along. Our unflinching response to the NUSA's blatant provocation proved effective.",
                            "maleVariant": "",
                            "stringId": "3395634287276826624"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'm being framed – isn't it obvious? The NUSA's been out for my blood for years.",
                            "maleVariant": "",
                            "stringId": "3470880000792961024"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What transpired in Dogtown is no trivial matter. I urge you all to consider the true motives behind this… spectacle.",
                            "maleVariant": "",
                            "stringId": "3470882295999995904"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Now, I'm not without flaws. But another thing I'm not is suicidal. An attempt on the life of the head of the NUSA?",
                            "maleVariant": "",
                            "stringId": "3470883160731262976"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Will they stop there? No. You, good people of Night City, are next in the fucking line.",
                            "maleVariant": "",
                            "stringId": "3470887239725457408"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The NUSA will take any excuse it can get to send forces into Dogtown.",
                            "maleVariant": "",
                            "stringId": "3470888729793896448"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Mark my words… another war's on its way.",
                            "maleVariant": "",
                            "stringId": "3470890842313826304"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This is Kurt Hansen. You are now in Dogtown.",
                            "maleVariant": "",
                            "stringId": "3489790704098844672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Rule two – no corps! You're just not welcome!",
                            "maleVariant": "",
                            "stringId": "3489791848271421440"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Rule one – no NCPD! Badges'll get a bullet to the brain!",
                            "maleVariant": "",
                            "stringId": "3489793122047991808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Rule three – my people keep the peace in the district and ensure its security. They are the law.",
                            "maleVariant": "",
                            "stringId": "3489794109202604032"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Follow the rules or face the consequences.",
                            "maleVariant": "",
                            "stringId": "3489809289412190208"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention all residents! This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3489829697553276928"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "A terrorist presence has been detected in this sector. I'm introducing additional security measures.",
                            "maleVariant": "",
                            "stringId": "3489830921618956288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Stay in your homes.",
                            "maleVariant": "",
                            "stringId": "3489834730416087040"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Report any suspicious activity!",
                            "maleVariant": "",
                            "stringId": "3489835740589039616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3499852020599595008"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "If you're hearing this, you've come to the right place.",
                            "maleVariant": "",
                            "stringId": "3499853481475678208"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "NCPD? Gang wars? Not in Dogtown.",
                            "maleVariant": "",
                            "stringId": "3499854249570181120"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Taxes? Who the fuck needs 'em?",
                            "maleVariant": "",
                            "stringId": "3499854880578052096"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "In Dogtown, I keep your business safe – as long as you abide by my rules.",
                            "maleVariant": "",
                            "stringId": "3499855077676785664"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sound fair to you? I thought so. Good.",
                            "maleVariant": "",
                            "stringId": "3499855540610506752"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No invite? Keep out. If you know what's good for you.",
                            "maleVariant": "",
                            "stringId": "3518791134124285952"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention all Dogtown residents! This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3518791638497730560"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "They will be welcomed at the Black Sapphire. The event is invitation-only.",
                            "maleVariant": "",
                            "stringId": "3518795529352224768"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What else can I say...",
                            "maleVariant": "",
                            "stringId": "3518815715799179264"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Rosalind, if you're ever really in Dogtown, we're ready to welcome you with open arms.",
                            "maleVariant": "",
                            "stringId": "3518821033505562624"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Attention all residents! This is Kurt Hansen.",
                            "maleVariant": "",
                            "stringId": "3537678408316461056"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Enemy collaborators have infiltrated Dogtown.",
                            "maleVariant": "",
                            "stringId": "3537679968412999680"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That means NUSA spies and agitators are in your midst.",
                            "maleVariant": "",
                            "stringId": "3537681926381215744"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Keep your eyes peeled. You see anything suspicious, report it.",
                            "maleVariant": "",
                            "stringId": "3537682715413684224"
                        }
                    ]
                }
            }
        },
        "EmbeddedFiles": []
    }
}