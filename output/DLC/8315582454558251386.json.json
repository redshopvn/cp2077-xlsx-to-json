    "Data": {
        "Version": 195,
        "BuildVersion": 0,
        "RootChunk": {
            "$type": "JsonResource",
            "cookingPlatform": "PLATFORM_PC",
            "root": {
                "HandleId": "0",
                "Data": {
                    "$type": "localizationPersistenceSubtitleEntries",
                    "entries": [
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hey, hey! Careful with that. I'm linked to the 'runner, see?",
                            "maleVariant": "",
                            "stringId": "2759157992353554432"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sure are confident.",
                            "maleVariant": "",
                            "stringId": "2760678434448990212"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wanna stop me? Try.",
                            "maleVariant": "",
                            "stringId": "2760678885034680320"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Who’s gonna stop me? You?",
                            "maleVariant": "",
                            "stringId": "2760680387971244032"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Your plan kills my client.",
                            "maleVariant": "",
                            "stringId": "2760681460085678080"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How can you be sure that'll work?",
                            "maleVariant": "",
                            "stringId": "2760683271706238976"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Because I'm a professional. Just like you.",
                            "maleVariant": "",
                            "stringId": "2957978391965982720"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Think he cares about those poor saps the Voodoo Boys are hacking? Last thing on his mind.",
                            "maleVariant": "",
                            "stringId": "2957980222242807808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fine, finish your op.",
                            "maleVariant": "",
                            "stringId": "3013028737008070660"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Here to protect my client.",
                            "maleVariant": "",
                            "stringId": "3013029409338224640"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fine, keep doin' your thing here. Just know if you fuck this up, you’re next on my shitlist.",
                            "maleVariant": "",
                            "stringId": "3013031294291996672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Guess you'll have to manage without Milko.",
                            "maleVariant": "",
                            "stringId": "3013034019079647232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Don't want 'im to catch a ricochet, huh?",
                            "maleVariant": "",
                            "stringId": "3093856028172283904"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Pulling Milko's plug will not save Indira's life. He'll just be replaced.",
                            "maleVariant": "",
                            "stringId": "3211644635745677312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Company men like him – all the same. Just out to be the next fake-smilin' employee of the month.",
                            "maleVariant": "",
                            "stringId": "3211665348191907840"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You should go. The others could arrive any minute.",
                            "maleVariant": "",
                            "stringId": "3223172884820717568"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Go. I have to get back to work.",
                            "maleVariant": "",
                            "stringId": "3223173338141093888"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "He’s a dead man anyway.",
                            "maleVariant": "",
                            "stringId": "3275165879385890820"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Whoever you are, I advise you to step back.",
                            "maleVariant": "",
                            "stringId": "3275253731985629184"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Netrunner, blackmailer, murderer. He’s my hit.",
                            "maleVariant": "",
                            "stringId": "3275253775858049028"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gimme one good reason.",
                            "maleVariant": "",
                            "stringId": "3275256283296866308"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I can.",
                            "maleVariant": "",
                            "stringId": "3275256545944182784"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "And my name’s Lizzy Wizzy.",
                            "maleVariant": "",
                            "stringId": "3275292842561806340"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Maybe. Do you know Bryce Mosley?",
                            "maleVariant": "",
                            "stringId": "3275293019980865536"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I’m Agent Alan Noël, NetWatch, organized crime specialist.",
                            "maleVariant": "",
                            "stringId": "3275293485884153856"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "An undercover NetWatch agent?",
                            "maleVariant": "",
                            "stringId": "3275296540612534272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "A real pleasure, Alan. My chooms call me Lizzy Wizzy.",
                            "maleVariant": "",
                            "stringId": "3275299254327222272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "First I've seen a NetWatch agent ditch the tie and button-up to get his hands dirty, live with the enemy.",
                            "maleVariant": "",
                            "stringId": "3275299294005338116"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Met a choom of yours recently. Likewise sniffin' around the Voodoo Boys.",
                            "maleVariant": "",
                            "stringId": "3275299331502415872"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Did you greet him with a gun too?",
                            "maleVariant": "",
                            "stringId": "3275311653042167808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I know who you are. Everyone at the Igloo has been talkin' about what you did in Pacifica.",
                            "maleVariant": "",
                            "stringId": "3275314001399422976"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Well, got fingers crossed for ya. But don't count on my help. Safety of my client's my only priority.",
                            "maleVariant": "",
                            "stringId": "3275344991433957376"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This client of yours... a corpo? Engineering type? That fits the Voodoos typical target profile.",
                            "maleVariant": "",
                            "stringId": "3275347898606723072"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We tried recruitment. More than once. None ever agreed, no matter our terms. We underestimated their loyalty to each other.",
                            "maleVariant": "",
                            "stringId": "3275354118642114560"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Well, Alan, NetWatch agent or not, if you stand in my way I'll be forced to remove you from it.",
                            "maleVariant": "",
                            "stringId": "3275362438195093504"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The difference is, I intend to restore order, whereas you offer only more chaos.",
                            "maleVariant": "",
                            "stringId": "3275378356736401408"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You think I don't know that?",
                            "maleVariant": "",
                            "stringId": "3275382207644344320"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Why do the Voodoo Boys want this tech?",
                            "maleVariant": "",
                            "stringId": "3275386631276109828"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'm still missin' the \"why\". They wanna open a museum, sell tickets?",
                            "maleVariant": "Still missin' the \"why\". They wanna open a museum, sell tickets?",
                            "stringId": "3275387541406523392"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Ya can't just find this stuff lyin' around the streets.",
                            "maleVariant": "",
                            "stringId": "3275387595412381700"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No, but you can beneath them. Dogtown was built on an old Militech bunker complex. Why do you think they built that wall?",
                            "maleVariant": "",
                            "stringId": "3275389087259205632"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "There are plenty of people willing to pay good money for old hardware.",
                            "maleVariant": "",
                            "stringId": "3275390362193403904"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Stand back.",
                            "maleVariant": "",
                            "stringId": "3282521182276329472"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Milko’s a corpse already, but you still got a choice.",
                            "maleVariant": "",
                            "stringId": "3282541443432226816"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You’d only waste your bullets on 'im. Trust me.",
                            "maleVariant": "",
                            "stringId": "3282623505543634944"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "He has some dirt under his nails, yes, but Milko is a no one. A small fish in a big pond.",
                            "maleVariant": "",
                            "stringId": "3282623933060653056"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Months I've suffered this place, holed up with these scabs on society. I aim to expose their web of blackmailers and murderers, like Milko.",
                            "maleVariant": "",
                            "stringId": "3282784554183991296"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Good luck with that.",
                            "maleVariant": "",
                            "stringId": "3282797171606724612"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Milko's your source?",
                            "maleVariant": "",
                            "stringId": "3282800801105747968"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This Milko guy work for you? Your source?",
                            "maleVariant": "",
                            "stringId": "3282809297893236764"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I piggyback whenever he jacks into the Net, try to pin down who's feeding him orders.",
                            "maleVariant": "",
                            "stringId": "3282809297893236776"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Doubtful.",
                            "maleVariant": "",
                            "stringId": "3284025214845231104"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "OK, Agent Alan NoëL, NetWatch, organized crime specialist. Get outta my way.",
                            "maleVariant": "",
                            "stringId": "3284025754853482496"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "At least let me unplug from Milko.",
                            "maleVariant": "",
                            "stringId": "3284026601314361344"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Agent Mosley? He speaks highly of you.",
                            "maleVariant": "",
                            "stringId": "3284031100007391232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I can prove it.",
                            "maleVariant": "",
                            "stringId": "3284031265397186564"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It's why I'm not gonna waste my breath on a corpo this time around.",
                            "maleVariant": "That's why I'm not gonna waste my breath on a corpo this time 'round.",
                            "stringId": "3284054871929954304"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You came to an understanding with our man back then.",
                            "maleVariant": "",
                            "stringId": "3284060286256324608"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Yeah, I thought so too. Till I learned your buddy secretly injected a tracking program in me. Class act.",
                            "maleVariant": "",
                            "stringId": "3284060741086650368"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "But to do that, I have to find out who's pulling the strings first.",
                            "maleVariant": "",
                            "stringId": "3284108144674250752"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "He started it. Tried to scramble my brain first.",
                            "maleVariant": "",
                            "stringId": "3284115599579848704"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No need. I like corpos even less than I do gangoons.",
                            "maleVariant": "",
                            "stringId": "3284124630654558208"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The Voodoos get their hands on old military tech. They wanna dust it off, get it running, which means they need experts to reverse-engineer it.",
                            "maleVariant": "",
                            "stringId": "3284164396716564480"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Where'd they get the tech from?",
                            "maleVariant": "",
                            "stringId": "3284170541573251072"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I have reports of victims all along the coast. Milko is only one of the 'runners working for the Voodoo Boys.",
                            "maleVariant": "",
                            "stringId": "3284172126181302272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I came across some combat bots.",
                            "maleVariant": "",
                            "stringId": "3284177346428837888"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Smashed a few bots along the way. Those old military models?",
                            "maleVariant": "",
                            "stringId": "3284181972930699264"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Mhm. And the Voodoo Boys won't be happy to lose them. But they do have more.",
                            "maleVariant": "",
                            "stringId": "3284182955706458112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "As soon as I find the head behind their operation, I can chop it off and end this for good.",
                            "maleVariant": "",
                            "stringId": "3284185441569775616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fuck! Do you have any idea what you just did?!",
                            "maleVariant": "",
                            "stringId": "3285527314930507776"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You won't regret it. I give my word.",
                            "maleVariant": "",
                            "stringId": "3285529160625594368"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Developing countries rarely see better than bootleg corptech. A lot of buyers might not even realize the Voodoos' product is outdated, or even obsolete.",
                            "maleVariant": "",
                            "stringId": "3285594490567827456"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It started as a secret military project back in the twenties. A total failure. Militech cut their losses and filled it with quicklime, but some persistent groups have managed to scavenge the labs, even today.",
                            "maleVariant": "",
                            "stringId": "3285611178512261120"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I’m sure we can work something out.",
                            "maleVariant": "",
                            "stringId": "3286725081048719360"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You just ruined a NetWatch operation costing in the tens of thousands.",
                            "maleVariant": "",
                            "stringId": "3286732930202116096"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "To you, these “victims” are numbers in a file, no names, no faces. Well, my client does have a name – Indira Barazza. And she'll die if I don't do my job.",
                            "maleVariant": "",
                            "stringId": "3286745408021635072"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Mhmmm. Either help with the hackin' or get hacked, huh? What's the angle there?",
                            "maleVariant": "",
                            "stringId": "3286935812864122880"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Give me a chance. Do you realize how many more people you can save this way?",
                            "maleVariant": "",
                            "stringId": "3286942743766933504"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I need Milko. His death alone won't change anything, understand?",
                            "maleVariant": "",
                            "stringId": "3286979207519182848"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Do not do this!",
                            "maleVariant": "",
                            "stringId": "3298371804179374080"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You won't do it, right?",
                            "maleVariant": "",
                            "stringId": "3298372176230916100"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You can't kill Milko. Not yet.",
                            "maleVariant": "",
                            "stringId": "3326224723717038156"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fine. But so much as a twitch and you're a flatline.",
                            "maleVariant": "",
                            "stringId": "3326225303453736960"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "That's better. Now listen to me.",
                            "maleVariant": "",
                            "stringId": "3326234142966194176"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Aaaagh!",
                            "maleVariant": "",
                            "stringId": "3417283197223366656"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Kick]",
                            "maleVariant": "",
                            "stringId": "3896884382817542148"
                        }
                    ]
                }
            }
        },
        "EmbeddedFiles": []
    }
}