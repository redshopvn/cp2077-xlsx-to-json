    "Data": {
        "Version": 195,
        "BuildVersion": 0,
        "RootChunk": {
            "$type": "JsonResource",
            "cookingPlatform": "PLATFORM_PC",
            "root": {
                "HandleId": "0",
                "Data": {
                    "$type": "localizationPersistenceSubtitleEntries",
                    "entries": [
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Leavin' for good. Vaya con Dios.",
                            "maleVariant": "",
                            "stringId": "3114464713326964740"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Ta ta, Night City.",
                            "maleVariant": "",
                            "stringId": "3114465151229079556"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Await contact. I'll ring you when the surgeons're ready.",
                            "maleVariant": "",
                            "stringId": "3114664433131433984"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'm needed in D.C. Or so I'm told.",
                            "maleVariant": "",
                            "stringId": "3121495671057063936"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fear. Song was scared you'd give 'er up to Myers.",
                            "maleVariant": "",
                            "stringId": "3282882510949019648"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Long-ass way to drive, choom.",
                            "maleVariant": "Hh. Long-ass way to drive, choom.",
                            "stringId": "3505624410927570944"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Well, trains just rub me wrong…",
                            "maleVariant": "",
                            "stringId": "3505624622874140672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Medkit in the car stocked?",
                            "maleVariant": "",
                            "stringId": "3505637321616039936"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Mhm. After ten damn years…",
                            "maleVariant": "",
                            "stringId": "3506858171623186432"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Shame – at havin' betrayed you…",
                            "maleVariant": "",
                            "stringId": "3506862114235392000"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Cunning. Knew she couldn't dupe you, duped me easy.",
                            "maleVariant": "",
                            "stringId": "3506862508164423680"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hope – that I'd understand her… predicament.",
                            "maleVariant": "",
                            "stringId": "3506862774603390976"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Song didn't trust you. Knew you'd prove Myers' loyal hound in the end.",
                            "maleVariant": "",
                            "stringId": "3506863916343271424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Poor fuckin' bastard…",
                            "maleVariant": "",
                            "stringId": "3506896011946008576"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Coulda done it already, long ago.",
                            "maleVariant": "",
                            "stringId": "3506896436308910080"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Choom lost everything, literally. Just hasn't realized it… yet.",
                            "maleVariant": "",
                            "stringId": "3506896859514183680"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Nick o' time. Minutes on you'd've found my tracks, that's it.",
                            "maleVariant": "",
                            "stringId": "3507208108546367488"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Besides which, I need time to think things through. Lots of things, lots of time.",
                            "maleVariant": "",
                            "stringId": "3509848602246029312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I get it – drive'll be a pilgrimage into the depths of Sol Reed.",
                            "maleVariant": "",
                            "stringId": "3509850872941551616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Your very own penitential path, this drive?",
                            "maleVariant": "",
                            "stringId": "3509870523088019456"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Open road for the quintessential introvert. To solve some problems on your own.",
                            "maleVariant": "",
                            "stringId": "3509877967675629568"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This self-punishment? A penitential path, cheap motels as contemplation stations?",
                            "maleVariant": "This self-punishment? A penitential path, cheap motels as, heh, contemplation stations?",
                            "stringId": "3509878221582016512"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "OK, Reed, what's on your mind? Whatcha need from me?",
                            "maleVariant": "",
                            "stringId": "3509887043193184256"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "C'mon. Song set you up to die seven years ago. Had to feel some shame…",
                            "maleVariant": "",
                            "stringId": "3509945960547917824"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Song knew she couldn't take you for a ride. You just knew her too well, you two'd been close.",
                            "maleVariant": "",
                            "stringId": "3509946662456303616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Song sensed I'd understand. What with death sentences hangin' over both our heads...",
                            "maleVariant": "",
                            "stringId": "3509946924935847936"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You in a hurry?",
                            "maleVariant": "",
                            "stringId": "3509962512764674048"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "In a hurry to get outta Dodge? Shoulda warned me.",
                            "maleVariant": "In a hurry to get outta Dodge? Mh. Shoulda warned me.",
                            "stringId": "3509963298005491712"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We might've just missed each other. Maybe I was hoping for that.",
                            "maleVariant": "",
                            "stringId": "3509968671647113216"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Feel sorry for 'im? You?",
                            "maleVariant": "",
                            "stringId": "3510016339308093440"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gonna hang himself. You'll see.",
                            "maleVariant": "",
                            "stringId": "3510017204274241536"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Stop spewin' crap.",
                            "maleVariant": "",
                            "stringId": "3510017744030834688"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "His president proved herself a war criminal.",
                            "maleVariant": "",
                            "stringId": "3510023475262038016"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "He'll hang himself.",
                            "maleVariant": "",
                            "stringId": "3510025372966494208"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fuck off, Johnny. Not even close to funny, that.",
                            "maleVariant": "",
                            "stringId": "3510025608568938496"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Had seven years to do that and didn't. He's got a chance to turn his life around now.",
                            "maleVariant": "",
                            "stringId": "3510025980519817216"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Can't. Promised he'd help me…",
                            "maleVariant": "",
                            "stringId": "3510032331014840320"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sure hope he sets up that surgery for me first…",
                            "maleVariant": "",
                            "stringId": "3510033181921681408"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You take care, V.",
                            "maleVariant": "",
                            "stringId": "3510060757943754752"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I can't sleep. Tryin' to break things down. Keep askin' myself, and... I don't know, but maybe you do.",
                            "maleVariant": "",
                            "stringId": "3510075603514990592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Glove compartment still stocked with aspirin?",
                            "maleVariant": "",
                            "stringId": "3510088841761824768"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "First we met – \"black Thorton Merrimac, three minutes, front seat.\"",
                            "maleVariant": "",
                            "stringId": "3510089336354152448"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Yeah. Old habit, hard to kick.",
                            "maleVariant": "",
                            "stringId": "3510095069883834368"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Not all of 'em. I did call you, drag you out here.",
                            "maleVariant": "",
                            "stringId": "3510146418684108800"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I ain't no saint – that's sure.",
                            "maleVariant": "",
                            "stringId": "3510146897958838272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hadn't thought to call her any time over that seven-year span.",
                            "maleVariant": "",
                            "stringId": "3510173107023683588"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Was ordered to hunt her, but I helped 'er instead.",
                            "maleVariant": "",
                            "stringId": "3510174013714767872"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "She had orders, followed 'em. And the shit I endured subsequent? I never put that on 'er.",
                            "maleVariant": "",
                            "stringId": "3510175716618969088"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Think it's true? Her now was not the woman I worked with way back when.",
                            "maleVariant": "",
                            "stringId": "3510177409305845760"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sentence was carried out in a way in my case. So Mi would've remembered that.",
                            "maleVariant": "",
                            "stringId": "3510228912724942848"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Vanished from 'er life for seven years. Left 'er to her own devices.",
                            "maleVariant": "Vanished from 'er life for seven years. Left her to her own devices.",
                            "stringId": "3510229498954424320"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hh, can only tell ya what I think's right. Don't have any cut 'n' dry answers for ya.",
                            "maleVariant": "Can only tell ya what I think's right. Don't have any cut 'n' dry answers for ya.",
                            "stringId": "3510229591094894592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Oughta just ask her yourself, really.",
                            "maleVariant": "",
                            "stringId": "3510235162724442112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "This one time, just let it go.",
                            "maleVariant": "",
                            "stringId": "3510236022539984900"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Want proof of loyalty? No better proof than that.",
                            "maleVariant": "",
                            "stringId": "3510504419139952640"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Contact would've been unwise, so… impossible. I played the role I had to.",
                            "maleVariant": "",
                            "stringId": "3510517125330935808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Song knew the truth, though. Knew full well you were no traitor.",
                            "maleVariant": "",
                            "stringId": "3510518048379805696"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Well, as I saw it, I was protectin' her.",
                            "maleVariant": "",
                            "stringId": "3510519336182128640"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How about askin' her yourself? You'll reach D.C., eventually. She's already there…",
                            "maleVariant": "",
                            "stringId": "3510521484672409600"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Know what your probem is? Ya find it damn hard to just let go.",
                            "maleVariant": "",
                            "stringId": "3510526174659256320"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Ah, opportunity's not likely to be there…",
                            "maleVariant": "",
                            "stringId": "3510527366714007552"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "And honesty much appreciated, V.",
                            "maleVariant": "",
                            "stringId": "3510545732430372864"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Got regrets? Learn to live with 'em.",
                            "maleVariant": "",
                            "stringId": "3510554859101659136"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Might well've botched some choices you faced in life. Not my place to judge.",
                            "maleVariant": "",
                            "stringId": "3510559894850150400"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "His \"friend\" flipped him a fuckin' club-sized bird.",
                            "maleVariant": "",
                            "stringId": "3537533618962735104"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "His ideals – bottom of a cesspit, chokin' on shit.",
                            "maleVariant": "",
                            "stringId": "3537534349174284288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "\"Black Thorton Merrimac.\"",
                            "maleVariant": "",
                            "stringId": "3546190058405404672"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "So Mi, up there in Space Force 1… she raised you on the holo, not me – why?",
                            "maleVariant": "",
                            "stringId": "3546217175251853312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Time for me to fly.",
                            "maleVariant": "",
                            "stringId": "3546239854457561088"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Dunno, maybe. Guy's bankrupt, all outta hope.",
                            "maleVariant": "",
                            "stringId": "3546245305911910400"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Oh, he will. Guy's honorable. Possibly his biggest problem.",
                            "maleVariant": "",
                            "stringId": "3546247247153242112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Open to some advice? Stop torturin' yourself with shit you can't affect, let alone control.",
                            "maleVariant": "Open to some advice? Stop torturing yourself with shit you can't affect, let alone control.",
                            "stringId": "3547734741967347712"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hm.",
                            "maleVariant": "",
                            "stringId": "3547737253499805696"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "One thing's sure, though – consequences of your mistakes? Gotta learn to live with 'em.",
                            "maleVariant": "One thing's sure, though – consequences of your mistakes? Tsk. Gotta learn to live with 'em.",
                            "stringId": "3547742768606912512"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Stop thinkin' about Songbird. Doesn't deserve it.",
                            "maleVariant": "",
                            "stringId": "3550778646979846144"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Know why you're not gettin' it? 'Cause you assume she was driven by 'er feelings. Well, wrong.",
                            "maleVariant": "Know why you're not gettin' it? 'Cause you assume she was driven by 'er feelin's. Well, wrong.",
                            "stringId": "3550813253645279232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Song ran a straight con. Used us both.",
                            "maleVariant": "",
                            "stringId": "3550816184994013184"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Practicality – calculated her chances, didn't listen to her heart.",
                            "maleVariant": "",
                            "stringId": "3550821261074931712"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Just wasn't about friendship, trust or loyalty for her.",
                            "maleVariant": "",
                            "stringId": "3550839969801478144"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'm hearin' resentment, and I understand. But we're not the victims in all this.",
                            "maleVariant": "",
                            "stringId": "3550843274728480768"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No problems with your memory.",
                            "maleVariant": "",
                            "stringId": "3550895161154641920"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I'm left wondering, though – if she'd raised me on the holo then, would I have made the right choice?",
                            "maleVariant": "",
                            "stringId": "3552000041726418944"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Reed, just forget Songbird. Did a lot more for 'er than she deserved, really.",
                            "maleVariant": "Reed, just forget Songbird. Did a lot more for her than she deserved, really.",
                            "stringId": "3552030547000074240"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Time to think about yourself. Gettin' a fresh start. It's rare, not many peeps get 'em.",
                            "maleVariant": "Time to think about yourself. Gettin' a fresh start. That's rare, not many peeps get 'em.",
                            "stringId": "3552035547432525824"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gonna visit Songbird?",
                            "maleVariant": "",
                            "stringId": "3576617609460097024"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Songbird had her surgery yet? Gonna see 'er?",
                            "maleVariant": "",
                            "stringId": "3576618243152322560"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "So Mi's in good hands. Has a lot of rehab to look forward to. Be a while before she's… back bein' herself.",
                            "maleVariant": "",
                            "stringId": "3576897227568095232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Then what? Congressional hearings?",
                            "maleVariant": "An' then what? Congressional hearings?",
                            "stringId": "3576898102248251392"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Myers is too pragmatic for that. She'll reinstate So Mi to the FIA, let 'er continued service be her penance.",
                            "maleVariant": "",
                            "stringId": "3576898398466777088"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Don't mention it.",
                            "maleVariant": "",
                            "stringId": "3579782140776587268"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You deserved it, Sol.",
                            "maleVariant": "",
                            "stringId": "3579782309840592896"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Best o' luck, Sol.",
                            "maleVariant": "",
                            "stringId": "3579782425536274432"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Bon voyage.",
                            "maleVariant": "",
                            "stringId": "3579783062969819136"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Wait]",
                            "maleVariant": "",
                            "stringId": "3956655966867927052"
                        }
                    ]
                }
            }
        },
        "EmbeddedFiles": []
    }
}