    "Data": {
        "Version": 195,
        "BuildVersion": 0,
        "RootChunk": {
            "$type": "JsonResource",
            "cookingPlatform": "PLATFORM_PC",
            "root": {
                "HandleId": "0",
                "Data": {
                    "$type": "localizationPersistenceSubtitleEntries",
                    "entries": [
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Should be safe here… Phew! I owe you one, truly. It was getting quite ugly back there.",
                            "maleVariant": "",
                            "stringId": "2776549988211060736"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sure thing. Not like I'm leavin' empty-handed, right?",
                            "maleVariant": "",
                            "stringId": "2776644328627335168"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Well, uh, I suppose I can take it from here. I'll tell my superiors you did one hell of a job.",
                            "maleVariant": "",
                            "stringId": "3272497799097184256"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "So you lied to me.",
                            "maleVariant": "",
                            "stringId": "3275412691563278340"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Why'd you betray the corp?",
                            "maleVariant": "",
                            "stringId": "3275414391917342720"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Prototype's my responsibility now.",
                            "maleVariant": "",
                            "stringId": "3297107171393658884"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fine. Watch yourself out there.",
                            "maleVariant": "",
                            "stringId": "3297107880180703232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fine, don't care. Next time you're in trouble – don't call me.",
                            "maleVariant": "",
                            "stringId": "3349403579983060992"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Ha ha hah...! Worry not my friend, I've learned my lesson. I'll catch my breath now – appreciate your help, smooth sailing, farewell!",
                            "maleVariant": "",
                            "stringId": "3349405382208380928"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Yeah, yeah. Watch yourself out there, Hasan.",
                            "maleVariant": "",
                            "stringId": "3349408466951200768"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No, no, wait! I, uh… I can't go back to Zetatech.",
                            "maleVariant": "",
                            "stringId": "3349425939599581184"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Know what? Gettin' on my nerves, choom. Ya get one answer – what the fuck's your deal?",
                            "maleVariant": "Y'know what? Gettin' on my nerves now, choom. Ya get one answer – what the fuck's your deal?",
                            "stringId": "3349432318414544896"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "My deal, uh… Agh, I don't intend to return to Zetatech. I planned to grab the prototype and vanish into Dogtown.",
                            "maleVariant": "",
                            "stringId": "3349434599058956288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How d'you think this is gonna go?",
                            "maleVariant": "",
                            "stringId": "3349443531903066112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "OK. You an' me – never met. Our story? Scavs got the schematics, prototype went AWOL. We clear?",
                            "maleVariant": "",
                            "stringId": "3349444371200073728"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You- really?! Y-yes, understood! I… Thank you!",
                            "maleVariant": "",
                            "stringId": "3349444371200073756"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Why you so hell-bent on kleppin' this tech?",
                            "maleVariant": "",
                            "stringId": "3349444517631614992"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Do you have even the faintest idea how much it's worth?",
                            "maleVariant": "",
                            "stringId": "3349444517648392192"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Fuckin' hazed me Hasan. Put my shit on the line for you, and you wanna leave me out to dry?",
                            "maleVariant": "",
                            "stringId": "3349445576710152192"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Now I delta. You? Find a big-ass rock, crawl under it. I mean it, Hasan. Zetatech gets half a whiff – ya dead.",
                            "maleVariant": "Now I delta. You? Find a big-ass rock, crawl under it. I mean it, Hasan. Zetatech gets half a whiff – you're dead.",
                            "stringId": "3349450466832408576"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wait... how in the hell d'you think that's gonna go? I tell my fixer sorry, shit outta luck, prototype bounced?",
                            "maleVariant": "",
                            "stringId": "3349455432368250896"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sly bastard bamboozled the corp and you in the process. Two-nothin' for Hasan. But I got a feelin' his luck ain't gonna last.",
                            "maleVariant": "",
                            "stringId": "3349470548052664328"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Uh… more'n your life, right?",
                            "maleVariant": "",
                            "stringId": "3349484207273402368"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Bah. I have no family, no friends. I gave my life to the corporation… The sacrifices I made… And for what?",
                            "maleVariant": "",
                            "stringId": "3349484385531322368"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What life? The bastards stole it!... The money from the prototype would have afforded me a fresh start…",
                            "maleVariant": "",
                            "stringId": "3349485799951294464"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I had a buyer, I was waiting on the signal to run... Then the scavs attacked and everything went to shit! Going back, it… it would be a death sentence.",
                            "maleVariant": "",
                            "stringId": "3356703237327544320"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Just say it got lost, moved, I don't know. I cut the signal. Your fixer can have the schematics – they're just as valuable!",
                            "maleVariant": "",
                            "stringId": "3356715269846196224"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Calling the fixer. No fast moves.",
                            "maleVariant": "",
                            "stringId": "3356741536054239232"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "We never met, understood?",
                            "maleVariant": "",
                            "stringId": "3357703025955168256"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Doin' it my way – callin' my fixer. You? Sit tight and shut up.",
                            "maleVariant": "",
                            "stringId": "3357705428586717192"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I was in a goddam scav cage! You would've left if I'd told you the truth!",
                            "maleVariant": "",
                            "stringId": "3357711907897204736"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Shoot Hasan] Sorry it had to end this way.",
                            "maleVariant": "",
                            "stringId": "3357737341485572096"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "So be it, Hasan… Sorry.",
                            "maleVariant": "",
                            "stringId": "3357739091298869260"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sly bastard survived an ambush, gave scavs the slip and bamboozled a corp. But at some point, everyone's luck runs out...",
                            "maleVariant": "",
                            "stringId": "3357757060301185040"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Brought it on himself. Let's just grab what we came for an' get outta here.",
                            "maleVariant": "",
                            "stringId": "3357757060317962260"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Right… OK…",
                            "maleVariant": "",
                            "stringId": "3357766756290519040"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sly bastard survived an ambush, gave scavs the slip and bamboozled a corp. But at some point, everyone's luck runs out...",
                            "maleVariant": "",
                            "stringId": "3357768297982451724"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Don't send him back to Zetatech. Like you to find him a mean chrome-doc, extract the tech an' let him walk.",
                            "maleVariant": "",
                            "stringId": "3357777398363840528"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "True. Consider this a gesture of good will. A solid foundation for our mutual cooperation.",
                            "maleVariant": "",
                            "stringId": "3357777398363840540"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Know all too well what those Zeta-pricks got in store for him. So I thought… y'know what? Fuck 'em.",
                            "maleVariant": "",
                            "stringId": "3357777398380617728"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Grab the implant, but let Hasan go.",
                            "maleVariant": "",
                            "stringId": "3357777404319752224"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hands? Sitch got tangled. Rescued a Zetatech hostage, sole survivor of a scav ambush.",
                            "maleVariant": "",
                            "stringId": "3357777404386861076"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I see. This impedes your assignment how, exactly?",
                            "maleVariant": "",
                            "stringId": "3357777404403638276"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Well, my choom here 'planted the prototype in his eye socket. Gonna need a ride.",
                            "maleVariant": "",
                            "stringId": "3357777404403638288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Inconvenient, but not disastrous. Coordinates, please. His escort back to Zetatech will arrive shortly.",
                            "maleVariant": "",
                            "stringId": "3357777404403638300"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You V? Hands sent me. Here for the schematics.",
                            "maleVariant": "",
                            "stringId": "3357777412641251348"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Yeah. Here ya go.",
                            "maleVariant": "",
                            "stringId": "3357777412641251360"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Preem, I'll take it from here.",
                            "maleVariant": "",
                            "stringId": "3357777412658028548"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I have to say I'm intrigued, V. Is this man worth the effort?",
                            "maleVariant": "",
                            "stringId": "3357888462745231360"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Done and done.",
                            "maleVariant": "",
                            "stringId": "3357922373844066312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I appreciate you having my back there. Truly.",
                            "maleVariant": "",
                            "stringId": "3357960011850440708"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Oh, no, no, no. You'll sit tight while I call my fixer, grab some transport. Sure he'll wanna handle the prototype – personally.",
                            "maleVariant": "",
                            "stringId": "3359691270301671424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I fucked up, OK?! If I'd known… please, if you send me back, I'm dead!",
                            "maleVariant": "",
                            "stringId": "3359786132942348288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Maybe, maybe not. But you definitely made a big mistake tryin' to con me.",
                            "maleVariant": "",
                            "stringId": "3359820919241433088"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You connivin' fuck. Shoulda schemed a little harder 'fore ya squirmed into a 'Saka-Zeta-Scavenger-shitwich.",
                            "maleVariant": "",
                            "stringId": "3359821321324191744"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Then… I give up. Kill me. I'm not being dramatic. I'd rather die by your hand than their tortures.",
                            "maleVariant": "",
                            "stringId": "3359833756647022592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Newsflash, choom – dead already. They'll find ya an' snuff ya before you walk with that tech.",
                            "maleVariant": "",
                            "stringId": "3359838737785675776"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sendin' 'em now.",
                            "maleVariant": "",
                            "stringId": "3359876008874336256"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "OK, sending the coordinates.",
                            "maleVariant": "",
                            "stringId": "3359884556429680640"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hmm… I assume you have a kind benefactor in mind to sponsor your friend's operation?",
                            "maleVariant": "",
                            "stringId": "3359887538210725888"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "C'mon Hands. Y'gotta have some favor to call in, won't cost ya a damn enny.",
                            "maleVariant": "",
                            "stringId": "3359888332309913600"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Wait for Hands' merc]",
                            "maleVariant": "",
                            "stringId": "3387006098202390532"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Stand]",
                            "maleVariant": "",
                            "stringId": "3407166056182820868"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wait, wait, you don't... We can make a--",
                            "maleVariant": "",
                            "stringId": "3440366530109366272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Screw over Zetatech? How?",
                            "maleVariant": "",
                            "stringId": "3440442086821715968"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How'd you imagine playin' a megacorp? Musta had some kinda plan.",
                            "maleVariant": "",
                            "stringId": "3440444859910971412"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I did, though I couldn't do it all on my own. I arranged a buyer at Dogtown's stadium. We would've met, were it not for these savages.",
                            "maleVariant": "",
                            "stringId": "3440444859927748616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Buyer coulda hazed you, though.",
                            "maleVariant": "Buyer could've hazed you, though.",
                            "stringId": "3440444859944525824"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "True. So I had the prototype implanted – it became my bargaining chip. He knew he couldn't harm me without jeopardizing the tech.",
                            "maleVariant": "",
                            "stringId": "3440444859944525848"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I was to vanish during the convoy's inspection at the border. I arranged an unmarked car, bribed the guards… But then the ambush, well…",
                            "maleVariant": "",
                            "stringId": "3440444859961303052"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I beg you, please don't send me back to the corp…",
                            "maleVariant": "",
                            "stringId": "3440515363678183424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Give schematics] Here.",
                            "maleVariant": "",
                            "stringId": "3440526427648155648"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Now to disable the bridge. Better not to have scavs on our backs, no? Hrgh!",
                            "maleVariant": "",
                            "stringId": "3478039294681014272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wow, a born engineer.",
                            "maleVariant": "",
                            "stringId": "3478042886095757316"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wow, weren't lying 'bout bein' an engineer, huh?",
                            "maleVariant": "",
                            "stringId": "3478046535526113280"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hey, as long as it stops working.",
                            "maleVariant": "",
                            "stringId": "3478048155919966208"
                        }
                    ]
                }
            }
        },
        "EmbeddedFiles": []
    }
}