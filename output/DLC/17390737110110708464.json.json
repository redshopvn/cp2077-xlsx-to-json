    "Data": {
        "Version": 195,
        "BuildVersion": 0,
        "RootChunk": {
            "$type": "JsonResource",
            "cookingPlatform": "PLATFORM_PC",
            "root": {
                "HandleId": "0",
                "Data": {
                    "$type": "localizationPersistenceSubtitleEntries",
                    "entries": [
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What you want is the protein - human casualties.",
                            "maleVariant": "",
                            "stringId": "2663713260714045440"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gotta make sure no one's watchin'. You can go.",
                            "maleVariant": "",
                            "stringId": "3043161985007726592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "If you believe in what the Crimson Harvest stands for - why shit on their parade?",
                            "maleVariant": "",
                            "stringId": "3192666304437530624"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No.",
                            "maleVariant": "",
                            "stringId": "3215789601945530372"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You're afraid of the Crimson Harvest?",
                            "maleVariant": "",
                            "stringId": "3215789894322073600"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Know how to do my job, thanks.",
                            "maleVariant": "",
                            "stringId": "3215792577300901888"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "No, well... I have a feeling. And not a whole lotta time.",
                            "maleVariant": "",
                            "stringId": "3215792632313393152"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "But you don't know the Crimson Harvest. They're everywhere and nowhere.",
                            "maleVariant": "",
                            "stringId": "3215793246309167104"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Said you're a terrorist with a heart o' gold.",
                            "maleVariant": "",
                            "stringId": "3215881022807453696"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Said you wanna turn against your terrorist chooms - sabotage their evil plans and save the day.",
                            "maleVariant": "",
                            "stringId": "3215881269952622592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Is it worth the risk? One botched attack ain't gonna stop your fellow fanatics. Opposite, in fact.",
                            "maleVariant": "",
                            "stringId": "3217425728045559808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Did Hands bring you up to speed?",
                            "maleVariant": "",
                            "stringId": "3222971619749900288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Prolly thought she'd be collectin' signatures and makin' peace-protest signs.",
                            "maleVariant": "",
                            "stringId": "3223013327623663616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Why? Scared the Crimson Harvest might know you're up to no good?",
                            "maleVariant": "",
                            "stringId": "3224861954779029504"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You're taking a needless risk.",
                            "maleVariant": "",
                            "stringId": "3225909561576038400"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gonna hang around in plain view?",
                            "maleVariant": "",
                            "stringId": "3226023458001821696"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I admire what you're doing.",
                            "maleVariant": "",
                            "stringId": "3227482786588172288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You Nele?",
                            "maleVariant": "",
                            "stringId": "3234682653571981312"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Nele Springer?",
                            "maleVariant": "",
                            "stringId": "3234683596401188864"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Waitin' for me, I take it?",
                            "maleVariant": "",
                            "stringId": "3234683681914658820"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I take it you're waiting for me.",
                            "maleVariant": "",
                            "stringId": "3234683846348152832"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Who's askin'?",
                            "maleVariant": "",
                            "stringId": "3234685923468156928"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You weren't followed?",
                            "maleVariant": "",
                            "stringId": "3234687236234977280"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "He said you're a terrorist.",
                            "maleVariant": "",
                            "stringId": "3234713629882753028"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You're betraying your organization.",
                            "maleVariant": "",
                            "stringId": "3234714577527021568"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How 'bout puttin' an end to hunger, starvation?",
                            "maleVariant": "",
                            "stringId": "3234735486753218560"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You got any clue what the Crimson Harvest even stands for?",
                            "maleVariant": "",
                            "stringId": "3234770339389415424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How did you join the Crimson Harvest?",
                            "maleVariant": "",
                            "stringId": "3234774258194751492"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The Crimson Harvest - they recruit you, or did you volunteer?",
                            "maleVariant": "",
                            "stringId": "3234778282428112896"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "People starving? Where? 'Cause dumpsters're overflowin' with food in Night City.",
                            "maleVariant": "",
                            "stringId": "3234778339705528324"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Grew up on a farm like most of my comrades. Corp's boot stampin' on our faces - that was our life. Until we realized we could fight back.",
                            "maleVariant": "",
                            "stringId": "3234826013590855680"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "V?",
                            "maleVariant": "",
                            "stringId": "3234894068706615296"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You city folk don't know shit. You think you're free? That slavery's ancient history? Oughta see how Biotechnica treats farmers.",
                            "maleVariant": "",
                            "stringId": "3234936457097814016"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Don't you plant bombs?",
                            "maleVariant": "",
                            "stringId": "3234950120647073792"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Take shard] What's this?",
                            "maleVariant": "",
                            "stringId": "3234971923192913920"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Take down a building - whooptie-fuckin'-do. Biotechnica just builds a new one.",
                            "maleVariant": "",
                            "stringId": "3234989984755130368"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Your bomb won't cause any good ol' fashioned destruction? Thought terrorists liked puttin' on a show.",
                            "maleVariant": "Your bomb won't cause any good ol' fashioned destruction? Thought terrorists liked to put on a show.",
                            "stringId": "3234991296129441792"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "So what do I do?",
                            "maleVariant": "",
                            "stringId": "3235006082007343104"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Not a lotta folks who'd take the risk you're takin' 'thout even expectin' a \"thank you.\" Respect.",
                            "maleVariant": "",
                            "stringId": "3235020441072869376"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Force 'em off their land to grow gene-modified wheat. For food, one'd think. Wrong. For CHOOH2 that's sold to Petrochem.",
                            "maleVariant": "",
                            "stringId": "3235037824651010048"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Name's V. Fixer sent me.",
                            "maleVariant": "",
                            "stringId": "3236125461566627840"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "At your service.",
                            "maleVariant": "",
                            "stringId": "3236130128266698752"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "And yet you're betraying them.",
                            "maleVariant": "",
                            "stringId": "3236159100975226880"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Uh, don't you guys plant bombs? Carry out mass killings?",
                            "maleVariant": "",
                            "stringId": "3236163231139930112"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "O-our people's hearts are in the right place, but they're blinded by their hatred of corps.",
                            "maleVariant": "",
                            "stringId": "3236194257111261184"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Nobody's starving in Night City.",
                            "maleVariant": "",
                            "stringId": "3236195131439095808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What do I have to do?",
                            "maleVariant": "",
                            "stringId": "3237313882859413504"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What's the point? You won't destroy any buildings.",
                            "maleVariant": "",
                            "stringId": "3237313882876190744"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The Crimson Harvest purchased a Net-nuke from Hansen. It's headed for Europe - specifically, Biotechnica's HQ in Berlin.",
                            "maleVariant": "",
                            "stringId": "3237314789466611724"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You'll make sure that doesn't happen.",
                            "maleVariant": "",
                            "stringId": "3237314789466611736"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Crimson Harvest - they're like family, and the things you'll do for family, well… Can keep you up at night, haunt you…",
                            "maleVariant": "",
                            "stringId": "3237628047620743168"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I… I never thought we'd start killing people. Things they had me do, I… I was naïve.",
                            "maleVariant": "",
                            "stringId": "3237652406628425728"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Think we'd better cut to the chase.",
                            "maleVariant": "",
                            "stringId": "3237662127146049536"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Find it, upload the program from this shard. Few days from now, when the bomb's detonated in Berlin, it'll neutralize the virus - stop its spread.",
                            "maleVariant": "",
                            "stringId": "3237705682812051456"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Too long I've sat on my ass agonizing over the shit I've done. That ends today.",
                            "maleVariant": "",
                            "stringId": "3237738061916184576"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "I support their ends, not their means.",
                            "maleVariant": "",
                            "stringId": "3240679026813476864"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "How does this Net-nuke work?",
                            "maleVariant": "",
                            "stringId": "3240723956785127424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "A Net-nuke? First I heard of it.",
                            "maleVariant": "",
                            "stringId": "3240724163144884224"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Clean, quiet, doe'n't distinguish 'tween class or creed. Real darn leveler.",
                            "maleVariant": "",
                            "stringId": "3240731738695442432"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Hook it up the subnet and it'll fry everyone unlucky enough to be connected.",
                            "maleVariant": "",
                            "stringId": "3240733557026889728"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "And I mean everyone. From executives, scientists, and rank-and-file corpos all the way down to cafeteria workers, janitors.",
                            "maleVariant": "",
                            "stringId": "3240733854201716736"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "\"Naïve.\" Eh, bit of an understatement, that.",
                            "maleVariant": "\"Naïve.\" Bit of an understatement, that.",
                            "stringId": "3246227111216062464"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Got enough to work with.",
                            "maleVariant": "",
                            "stringId": "3246278099708846084"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What did you do?",
                            "maleVariant": "",
                            "stringId": "3246278413526671360"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Sounds like you're tryin' to atone for something…",
                            "maleVariant": "",
                            "stringId": "3246279091594633216"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Guess I'll get to work then.",
                            "maleVariant": "",
                            "stringId": "3246283691286503424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "When it's done, I'd like that shard back.",
                            "maleVariant": "",
                            "stringId": "3246287044833878016"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Gonna keep it as a souvenir?",
                            "maleVariant": "",
                            "stringId": "3246288158220263424"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Can't be too careful. If the Crimson Harvest ever got their hands on it, I'm toast.",
                            "maleVariant": "",
                            "stringId": "3246288224087613440"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Maybe I'm doing it for myself?",
                            "maleVariant": "",
                            "stringId": "3246317537474289664"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Heh. You done prying?",
                            "maleVariant": "",
                            "stringId": "3246318809707356160"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Spare me the judgment.",
                            "maleVariant": "",
                            "stringId": "3246386111123464192"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "For the love of-- We're not terrorists!",
                            "maleVariant": "",
                            "stringId": "3255292013087989760"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Can't turn back time, but I can still change what happens here, now - the future.",
                            "maleVariant": "",
                            "stringId": "3256386136511520768"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Western part of the stadium used to house a sports therapy center 'fore Hansen turned it into an arms factory. Entry code's one-seven-oh-one. Memorize it.",
                            "maleVariant": "",
                            "stringId": "3262200462312906752"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "It's got assembly lines, warehouses - the works. Part you're interested in's an experimental lab in the old pool. S'where they keep the bomb.",
                            "maleVariant": "",
                            "stringId": "3262210839775977472"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Countin' on ya, V.",
                            "maleVariant": "",
                            "stringId": "3262220232701681664"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "[Take shard] How discreet.",
                            "maleVariant": "",
                            "stringId": "3264952227793903616"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Wow, discreet. New to this, are we?",
                            "maleVariant": "",
                            "stringId": "3265002001146486784"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Here.",
                            "maleVariant": "",
                            "stringId": "3267855438469046272"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "What's on the shard?",
                            "maleVariant": "",
                            "stringId": "3267857668077166592"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "One thing at a time, 'kay?",
                            "maleVariant": "",
                            "stringId": "3267857967214927872"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Didn't hire you to pen my biography.",
                            "maleVariant": "",
                            "stringId": "3318524140369387520"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Don't count on Biotechnica's thanks.",
                            "maleVariant": "",
                            "stringId": "3324562111392555008"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "If you think this'll win you Biotechnica's protection against the Crimson Harvest - you've got another thing comin'.",
                            "maleVariant": "",
                            "stringId": "3324562761073467400"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "You like the taste of CHOOH2? No? S'what I thought.",
                            "maleVariant": "",
                            "stringId": "3324586539857911808"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "The nomads also started out as farmers.",
                            "maleVariant": "",
                            "stringId": "3324597340291375104"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Farms are empty, but the roads are full. The first nomads were farmers too, y'know.",
                            "maleVariant": "",
                            "stringId": "3324614477143777280"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Mhm, heard all about clans takin' corp contracts. Kind of ironic, don't ya think?",
                            "maleVariant": "",
                            "stringId": "3324627944735154176"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Tough world out there. Gotta make a livin'.",
                            "maleVariant": "",
                            "stringId": "3324630469035073536"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "May be naïve, but not that naïve. Know a thing or two 'bout corps.",
                            "maleVariant": "",
                            "stringId": "3324648421025292288"
                        },
                        {
                            "$type": "localizationPersistenceSubtitleEntry",
                            "femaleVariant": "Know they ain't much different from the Crimson Harvest - just shut up, follow orders. Same leash, different collar.",
                            "maleVariant": "",
                            "stringId": "3374060679803084800"
                        }
                    ]
                }
            }
        },
        "EmbeddedFiles": []
    }
}