          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We return to this week\u0027s biggest story – the murder of Arasaka Corporation CEO Saburo Arasaka.",
              "maleVariant": "",
              "stringId": "1743065729017892864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The long-time head of the Japanese megacorporation was reported slain on the premises of the Konpeki Plaza corporate hotel while attending a family gathering.",
              "maleVariant": "",
              "stringId": "1743067415698526208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saburo Arasaka\u0027s son, Yorinobu Arasaka, has cast blame for his father\u0027s murder on the company\u0027s rivals, although he has not yet provided evidence supporting this claim.",
              "maleVariant": "",
              "stringId": "1743068079254196224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "According to market analysts, the CEO\u0027s death may have been a desperate attempt to divert attention away from the company\u0027s ongoing internal issues.",
              "maleVariant": "",
              "stringId": "1743068428371283968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "After the break, we ask experts how they see Arasaka rebounding from the crisis.",
              "maleVariant": "",
              "stringId": "1743068898670202880"
            }
          ]
        }
