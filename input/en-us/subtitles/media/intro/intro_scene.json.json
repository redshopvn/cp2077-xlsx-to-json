            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good morning, Night City!",
              "maleVariant": "",
              "stringId": "2039066195769614336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yesterday\u0027s body-count lottery rounded out to a solid \u0027n\u0027 sturdy thirty! Ten outta Heywood – thanks to unabated gang wars!",
              "maleVariant": "",
              "stringId": "2039101856863416320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One officer down, so I guess you\u0027re all screwed. \u0027Cause the NCPD will NOT let that go.",
              "maleVariant": "",
              "stringId": "2039101945061240832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got another blackout in Santo Domingo. Netrunners at it again, pokin\u0027 holes in the power grid!",
              "maleVariant": "",
              "stringId": "2039102328538066944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "While over in Westbrook, Trauma Team\u0027s scrapin\u0027 cyberpsycho victims off the pavement.",
              "maleVariant": "",
              "stringId": "2039102416819777536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And in Pacifica...",
              "maleVariant": "",
              "stringId": "2039102495404257280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well... Pacifica is still Pacifica.",
              "maleVariant": "",
              "stringId": "2039102603684409344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This has been your man, Stan. Join me for another day in our City of Dreams!",
