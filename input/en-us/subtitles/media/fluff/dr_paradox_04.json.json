    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Congratulations! If you\u0027re sitting and watching this, it means you survived.",
              "maleVariant": "",
              "stringId": "1796381664982507520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No lunatic broke into your home when you were sleeping. No piece o\u0027 trash held you at gunpoint on your way to the subway. No cyberpsycho shredded you to ribbons while you were buying groceries...",
              "maleVariant": "",
              "stringId": "1804079439000879104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The powers that be say there\u0027s nothing to be done – that there\u0027s no budget for police, that nobody knows what causes cyberpsychosis, that drug trafficking is inevitable.",
              "maleVariant": "",
              "stringId": "1804079567967338496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lies, I tell you. Dirty, stinking lies.",
              "maleVariant": "",
              "stringId": "1804079779393814528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t you get that\u0027s what they want? They want you to wet yourself everytime you hear footsteps behind you, whenever somebody knocks at your door. They want you to draw that iron and pull the trigger.",
              "maleVariant": "",
              "stringId": "1804079938945138688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But do you know what THEY\u0027RE afraid of? Trust. Trust between neighbors, citizens. Because that\u0027s what\u0027s stoppin\u0027 us from bein\u0027 allies \u0027stead of enemies.",
              "maleVariant": "",
              "stringId": "1804080095560450048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our society is broken. We\u0027re a heap of depressed, lonely, ego-obsessed individuals – and so we grab on to anyone who offers us a fantasy on a chip, a dream of a better life.",
              "maleVariant": "",
              "stringId": "1804080291535110144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We need to learn how to talk to each other. How to stop being afraid. How to trust.",
              "maleVariant": "",
              "stringId": "1804080461203095552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remember, change starts from within.",
              "maleVariant": "",
              "stringId": "1804080594179309568"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
