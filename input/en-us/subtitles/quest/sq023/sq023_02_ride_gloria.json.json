      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have you ever experienced anything that changed the meaning of existence? Changed your life?",
              "maleVariant": "",
              "stringId": "1373339725379457040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes.",
              "maleVariant": "",
              "stringId": "1373340545835651072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No.",
              "maleVariant": "",
              "stringId": "1373340960971083776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t feel like talking about it.",
              "maleVariant": "",
              "stringId": "1374792601532420096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can\u0027t correct murder.",
              "maleVariant": "",
              "stringId": "1374792717915967488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, any reason you\u0027re telling me this?",
              "maleVariant": "",
              "stringId": "1374792717932744716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A tale of repentance and rebirth? Gimme a break…",
              "maleVariant": "",
              "stringId": "1374792717932744732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can\u0027t say I have, no.",
              "maleVariant": "",
              "stringId": "1374793352900038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could say that, sure.",
              "maleVariant": "",
              "stringId": "1374793352916815872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t feel like talkin\u0027 about it.",
              "maleVariant": "",
              "stringId": "1374793442372931584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Twelve years ago I was sentenced to prison for armed robbery.",
              "maleVariant": "",
              "stringId": "1374793687890710528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Then you will understand exactly what I\u0027m trying to do.",
              "maleVariant": "",
              "stringId": "1374794501266919424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Too bad. We should all have a moment like that. I\u0027ve had mine.",
              "maleVariant": "",
              "stringId": "1374794635232989184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Then let me speak.",
              "maleVariant": "",
              "stringId": "1374794856390250496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And other sins. I shot and killed several people in cold blood, and it didn\u0027t mean anything to me at the time.",
              "maleVariant": "",
              "stringId": "1374795439700496384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But behind bars, you get all the time in the world to think. And I thought to correct the errors of my ways.",
              "maleVariant": "",
              "stringId": "1374795530834333696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still don\u0027t get why you\u0027re tellin\u0027 me this.",
              "maleVariant": "I still don\u0027t get why you\u0027re tellin\u0027 me this.",
              "stringId": "1374796745018232832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spelling mistakes you can correct. But murder\u0027s murder.",
              "maleVariant": "Spelling mistakes you can correct. But murder is murder.",
              "stringId": "1374796745035010048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A tale of repentance, redemption and rebirth? Been on TV thousands of times, probably. I\u0027m usually asleep before it\u0027s half over.",
              "maleVariant": "",
              "stringId": "1374796745051787264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why\u0027d you shoot \u0027em?",
              "maleVariant": "",
              "stringId": "1374797275983564800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why\u0027d you shoot \u0027em?",
              "maleVariant": "",
              "stringId": "1374797446591074304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I was holding a gun. I didn\u0027t like the way they looked at me, and I had the power to stop them.",
              "maleVariant": "",
              "stringId": "1374798802961879040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I still see their faces just before I... Ahem. Frozen in time. God... give me strength to never forget.",
              "maleVariant": "",
              "stringId": "1374799260996653056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can make amends to the families…",
              "maleVariant": "",
              "stringId": "1374800620806787072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want you to understand what I am about to do.",
              "maleVariant": "",
              "stringId": "1374800743548899328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This won\u0027t be TV. This will be entirely different.",
              "maleVariant": "",
              "stringId": "1374800956334329856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Spill already. What do you want from me?",
              "maleVariant": "",
              "stringId": "1374801127227052032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stay with me for the day. And I\u0027ll pay you what you were promised to kill me.",
              "maleVariant": "",
              "stringId": "1374801457134227456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh, excuse me – with what money?",
              "maleVariant": "",
              "stringId": "1374801588600492032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not mine. The studio\u0027s. And the studio will pay.",
              "maleVariant": "",
              "stringId": "1374801803298525184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not likin\u0027 this one bit. Lemme out, I\u0027ll be on my way.",
              "maleVariant": "",
              "stringId": "1374802162096066560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure, why not.",
              "maleVariant": "",
              "stringId": "1374802162112843776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Drop me off at the next corner, will ya?",
              "maleVariant": "",
              "stringId": "1374802162112843792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just till the end of the day?",
              "maleVariant": "",
              "stringId": "1374802162129620992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still got no idea what it is you want from me.",
              "maleVariant": "I still got no idea what it is you want from me.",
              "stringId": "1374802162146398220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still have no idea what you want me to do.",
              "maleVariant": "",
              "stringId": "1374802678230339584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We\u0027re almost at Gloria El-Ahmar\u0027s.",
              "maleVariant": "",
              "stringId": "1374803224110616576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Till the end of the day.",
              "maleVariant": "",
              "stringId": "1374805886336028672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No one will force you to stay.",
              "maleVariant": "",
              "stringId": "1374806005471039488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s easy money. Just, uh... give it a thought.",
              "maleVariant": "",
              "stringId": "1374806142490562560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Remain by my side and talk to me. That\u0027s it.",
              "maleVariant": "",
              "stringId": "1374806377354809344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I sense anything goin\u0027 wrong, I\u0027m out.",
              "maleVariant": "",
              "stringId": "1386324526545817600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just wish I could\u0027ve talked to Gloria, explained everything…",
              "maleVariant": "",
              "stringId": "1754758574545801216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If somehow I thought... I-I thought she\u0027d find it in her heart to forgive me…",
              "maleVariant": "",
              "stringId": "1754759797185417216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Give me a moment, I need to prepare.",
              "maleVariant": "",
              "stringId": "1772143330161823744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, here we go...",
              "maleVariant": "",
              "stringId": "1805121511547756544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck... the hell possessed me to get in…",
              "maleVariant": "",
              "stringId": "1805121740103770112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can lead others away from the path of darkness.",
              "maleVariant": "",
              "stringId": "1805236924935856128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know, still got that contract to kill you. What\u0027s to stop me from fulfilling?",
              "maleVariant": "",
              "stringId": "1805313140338483200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Me. One strange twitch, I kill you.",
              "maleVariant": "",
              "stringId": "1805314352962101248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, there\u0027s no reason to upset Vasquez. Your fixer will get what they want. I\u0027ll make sure of it.",
              "maleVariant": "",
              "stringId": "1805314734945755136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How you figure that?",
              "maleVariant": "",
              "stringId": "1805317958083829760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trust me. Everyone will be satisfied. I guarantee it.",
              "maleVariant": "",
              "stringId": "1805318077822820352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can\u0027t talk to Rachel?",
              "maleVariant": "",
              "stringId": "1805410341890842624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Could try taming Vasquez.",
              "maleVariant": "",
              "stringId": "1805410686511636480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you lonely?",
              "maleVariant": "",
              "stringId": "1805411707656888320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You don\u0027t like Rachel? She\u0027s got the gift of the gab, surely.",
              "maleVariant": "Don\u0027t like Rachel? She\u0027s got the gift of the gab, surely.",
              "stringId": "1805411928864481280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Try Vasquez. You can probably get him to speak if you dangle a dog treat in front of his nose.",
              "maleVariant": "",
              "stringId": "1805412002835226624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why? Are you lonely?",
              "maleVariant": "",
              "stringId": "1805412100596064256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027m merely a springboard for her career. Heh, a high-value asset.",
              "maleVariant": "",
              "stringId": "1805415443187916800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re the one I want to talk to.",
              "maleVariant": "",
              "stringId": "1805416469970313216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go fuck yourself.",
              "maleVariant": "",
              "stringId": "1805417130321534976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vasquez sees me as nothing but a liar, scum that should\u0027ve fried in the electric chair.",
              "maleVariant": "",
              "stringId": "1805417863167107072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re the one I want to talk to.",
              "maleVariant": "",
              "stringId": "1805419384927707136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes, I am.",
              "maleVariant": "",
              "stringId": "1805419849052610560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I truly need you.",
              "maleVariant": "",
              "stringId": "1805419905960927232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still on contract to kill you.",
              "maleVariant": "",
              "stringId": "1811090004730470400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who can blame her?",
              "maleVariant": "",
              "stringId": "1812492135626727428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Huh, time to grab a bite to eat.",
              "maleVariant": "",
              "stringId": "1812496194135941120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who could blame her? Not easy to forgive your son\u0027s murderer.",
              "maleVariant": "Who can blame her? It\u0027s not easy to forgive your son\u0027s murderer.",
              "stringId": "1812508839962501120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, probably would\u0027ve forgiven you if that had been me.",
              "maleVariant": "",
              "stringId": "1812508903984357376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gloria should have forgiven you.",
              "maleVariant": "",
              "stringId": "1812508984900870144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where we gonna eat?",
              "maleVariant": "",
              "stringId": "1812526066673672192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where\u0027re we eatin\u0027?",
              "maleVariant": "",
              "stringId": "1812529919594881024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uhh, PieZ.",
              "maleVariant": "",
              "stringId": "1812530472840355840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They do a great cup of coffee.",
              "maleVariant": "",
              "stringId": "1812531461219704832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stephenson, we all knew how that would end.",
              "maleVariant": "",
              "stringId": "1812532582743040000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0022If you forgive anyone their sins, they are forgiven. If you do not forgive them, they are not forgiven.\u0022 Pff... ugh.",
              "maleVariant": "",
              "stringId": "1812551452816072704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But you\u0027re not her, are you? Nobody killed your son.",
              "maleVariant": "",
              "stringId": "1812609010662662144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vasquez!",
              "maleVariant": "",
              "stringId": "1812609436887834624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really gonna submit, let yourself get crucified?",
              "maleVariant": "",
              "stringId": "1812610089655754756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "PieZ serves up a killer cheesecake.",
              "maleVariant": "",
              "stringId": "1812610929422524416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Making a braindance of a crucifixion? Really?",
              "maleVariant": "",
              "stringId": "1812611067364794368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You really wanna do this? Get nailed to a cross?",
              "maleVariant": "",
              "stringId": "1812611555397230592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Really aim to spin a virtu of your crucifixion?",
              "maleVariant": "You really aim to spin a virtu of your crucifixion?",
              "stringId": "1812611622774530048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a killer cheesecake at PieZ…",
              "maleVariant": "",
              "stringId": "1812611690168606720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is something I must do.",
              "maleVariant": "",
              "stringId": "1812612215916224512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes.",
              "maleVariant": "",
              "stringId": "1812619046004744192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ugh, yes, really.",
              "maleVariant": "",
              "stringId": "1812625832103403520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027ll be kickin\u0027 up a shitstorm...",
              "maleVariant": "",
              "stringId": "1812625944309424128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Haters gonna hate.",
              "maleVariant": "",
              "stringId": "1812626139747213312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "For if this won\u0027t shake the world to its core, and wake us from our collective nightmare, then nothing will.",
              "maleVariant": "",
              "stringId": "1812628354406834176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027m lactose intolerant.",
              "maleVariant": "",
              "stringId": "1812636581299249152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How \u0027bout vegan avocado ice cream, then?",
              "maleVariant": "",
              "stringId": "1812636780478357504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can\u0027t ever truly know Night City.",
              "maleVariant": "",
              "stringId": "1833431425571381252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How well do you know the city?",
              "maleVariant": "",
              "stringId": "1833432267233976320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why\u0027s it matter?",
              "maleVariant": "",
              "stringId": "1833433583926665216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why does it matter?",
              "maleVariant": "Why\u0027s it matter?",
              "stringId": "1833434232483504128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, just curious. Happen to like it out here is all.",
              "maleVariant": "",
              "stringId": "1833434516186226688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can\u0027t know Night City well, ever. It\u0027s always changing... always surprising. Part of its charm.",
              "maleVariant": "You can\u0027t know Night City well, ever. It\u0027s always changin\u0027, always surprisin\u0027. Part of its charm.",
              "stringId": "1833434680921710592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, true that.",
              "maleVariant": "",
              "stringId": "1833436625266827264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ehh, take Stephenson inside, grab us all a table.",
              "maleVariant": "",
              "stringId": "1857678486558760960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need a word with our stowaway here.",
              "maleVariant": "",
              "stringId": "1857680234023247872"
            }
          ]
        }
      }
    },
