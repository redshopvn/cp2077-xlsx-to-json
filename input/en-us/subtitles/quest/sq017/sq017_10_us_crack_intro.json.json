              "maleVariant": "",
              "stringId": "1833100496595447808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Later.",
              "maleVariant": "",
              "stringId": "1833100791455019008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] Let\u0027s get one with all of us.",
              "maleVariant": "",
              "stringId": "1924393575528083456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Snap picture]",
              "maleVariant": "",
              "stringId": "1924401018471526404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s, like, our new single for the North American leg of the tour.",
