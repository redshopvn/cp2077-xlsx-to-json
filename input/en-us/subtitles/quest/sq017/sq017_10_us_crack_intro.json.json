              "stringId": "1833094777611612160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure.",
              "maleVariant": "",
              "stringId": "1833100250473689088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
