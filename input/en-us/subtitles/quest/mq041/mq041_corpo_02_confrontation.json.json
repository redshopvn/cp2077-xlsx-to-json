      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Open]",
              "maleVariant": "",
              "stringId": "1824513606298771460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Empty… Heh. Damn.",
              "maleVariant": "",
              "stringId": "1826986893784260608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hello, V. Man, time flies.",
              "maleVariant": "",
              "stringId": "1827029372168445952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Inferred death just ain\u0027t what it used to be…",
              "maleVariant": "",
              "stringId": "1827036092215205888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Key word – inferred. But Abernathy is purging the dep. Purged me. They took my house, accounts, implants…",
              "maleVariant": "",
              "stringId": "1827037127067443200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But I\u0027m not done. Gonna prove she was wrong about me, shove it down her throat.",
              "maleVariant": "",
              "stringId": "1827037292323020800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, so you make your move steppin\u0027 on me. Gonna serve my head up on a silver platter – and you think Abernathy\u0027ll welcome you back, arms outstretched?",
              "maleVariant": "Ah, so you make your move steppin\u0027 on me. Gonna serve my head up on a silver platter? And you think Abernathy\u0027ll welcome you back, arms outstretched?",
              "stringId": "1827037660012486656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027m nobody to Abernathy.",
              "maleVariant": "",
              "stringId": "1827038380912680964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Listen, Frank. I\u0027m nobody to Abernathy. Prolly doesn\u0027t know I exist, if she ever knew.",
              "maleVariant": "",
              "stringId": "1827038599284924416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You had it in for her, V, were setting her up. People don\u0027t forget shit like that.",
              "maleVariant": "",
              "stringId": "1827038867972038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re pathetic.",
              "maleVariant": "",
              "stringId": "1827039328523395072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0027Member Joey Livani? Gonk from mergers and acquisitions? Pissed himself the day he was fired.",
              "maleVariant": "",
              "stringId": "1827045028112769024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What the fuck are you talking about?",
              "maleVariant": "",
              "stringId": "1827046239259992064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, wa\u0027n\u0027t me. That was Jenkins\u0027 thing. Used me as his tool.",
              "maleVariant": "No, that wasn\u0027t me. That was Jenkins\u0027 thing. Just used me as his tool.",
              "stringId": "1827047177727123456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Always were arrogant. No, no time to chat, too busy with something super important.",
              "maleVariant": "",
              "stringId": "1827050036732502016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa, about to piss yourself, too. Can smell it comin\u0027.",
              "maleVariant": "",
              "stringId": "1827062584110358528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck you sideways, V. You and this whole rotten city.",
              "maleVariant": "",
              "stringId": "1827063037481066496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You really don\u0027t care if I end you.",
              "maleVariant": "",
              "stringId": "1827063621915385856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta start over.",
              "maleVariant": "",
              "stringId": "1827074289288749060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not my problem.",
              "maleVariant": "",
              "stringId": "1827074554033217536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dunno. And frankly, don\u0027t give the slimmest shit.",
              "maleVariant": "",
              "stringId": "1827077277327695872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for your honesty, V. Good to know exactly where I stand.",
              "maleVariant": "",
              "stringId": "1827077962341425152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re right. Going back to Arasaka... it\u0027s suicide, one way or another.",
              "maleVariant": "",
              "stringId": "1827078894902976512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Huh. Miss the warmth of the corporate lap? That unique thrill of internal power struggles?",
              "maleVariant": "",
              "stringId": "1866521648498089984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Butt out.",
              "maleVariant": "",
              "stringId": "1874812905695006720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Butt out, not your biz.",
              "maleVariant": "",
              "stringId": "1874813692160561152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm, I get it, fine. I\u0027m gonna go take a piss.",
              "maleVariant": "",
              "stringId": "1874813914509004800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got a few scores to settle.",
              "maleVariant": "",
              "stringId": "1874816229446045696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What? Got a score to settle is all.",
              "maleVariant": "",
              "stringId": "1874818569548910592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A-hah. I knew it, I knew it, I knew it. Rat\u0027s still in the race.",
              "maleVariant": "",
              "stringId": "1874819332576694272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027tcha need to be somewhere else? Your piece need oilin\u0027, guitar need tunin\u0027 maybe?",
              "maleVariant": "Don\u0027tcha need to be somewhere else? Your piece need oiling, guitar needs tunin\u0027 maybe?",
              "stringId": "1874822824770981888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That\u0027s about the size of it. Now turn, hands on your head, hop to it.",
              "maleVariant": "",
              "stringId": "1874834177611059200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here\u0027s another idea - I kill you right here and now.",
              "maleVariant": "",
              "stringId": "1874835579884326912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shoulda checked my schedule. Got plans for this evening.",
              "maleVariant": "Shoulda checked my schedule. Got plans for this evenin\u0027.",
              "stringId": "1874836582775967744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Namely?",
              "maleVariant": "",
              "stringId": "1874839278388039680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0027Bout to find out.",
              "maleVariant": "",
              "stringId": "1874839595947184128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saw him as the most pathetic guy ever at Arasaka. Not so anymore. You just took that title.",
              "maleVariant": "",
              "stringId": "1874843852209442816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His knees were shakin\u0027 so much he couldn\u0027t even walk. Guards had to drag \u0027im out to the curb.",
              "maleVariant": "",
              "stringId": "1874845345230671872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go ahead, try.",
              "maleVariant": "",
              "stringId": "1874854347800891396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Want you to open your eyes.",
              "maleVariant": "",
              "stringId": "1874854663548096512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Go ahead an\u0027 try, Frank. C\u0027mon. Lemme see what you\u0027re capable of.",
              "maleVariant": "",
              "stringId": "1874855261186084864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Open your eyes, Frank. Arasaka threw you out like so much trash... and you\u0027re gonna go back and beg for mercy, knowing they have none to give?",
              "maleVariant": "",
              "stringId": "1874856565732069376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lots of people have gone back.",
              "maleVariant": "",
              "stringId": "1874857714988142592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lots – nah, some – sure. To a good brain cleansing, just in case. That what you really want?",
              "maleVariant": "",
              "stringId": "1874858488518463488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s a closed book, Frank. Gotta refigure your life. Game over, start again.",
              "maleVariant": "",
              "stringId": "1874859324527136768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Start again… Got nothing to start with, less!",
              "maleVariant": "",
              "stringId": "1874860415348166656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got iron and a full clip. Add a little gumption – you\u0027re good to go in NC.",
              "maleVariant": "",
              "stringId": "1874862006314782720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Keep it together, Frank.",
              "maleVariant": "Just keep it together, Frank.",
              "stringId": "1874862696210681856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, what? Back to the office? Recently fired employee walks in, gun in hand. They\u0027ll shoot you, then put me down.",
              "maleVariant": "So, what? Back to the office? Recently fired employee walks in, gun in hand? They\u0027ll shoot you, and then put me down.",
              "stringId": "1874899356508016640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe. But see, V, the problem is I have nothing to lose.",
              "maleVariant": "",
              "stringId": "1874946667971923968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Clearly won\u0027t change your mind, but... not about to die with you, either.",
              "maleVariant": "Clearly won\u0027t change your mind. But I\u0027m not about to die with you, either.",
              "stringId": "1874947542517862400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So what\u0027m I supposed to do? Huh? What the fuck can I do?",
              "maleVariant": "",
              "stringId": "1874950099248140288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sometimes the simplest solutions get it done.",
              "maleVariant": "",
              "stringId": "1874956064387289088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh. Fuuuck…",
              "maleVariant": "Heh. Ah, fuck…",
              "stringId": "1874956843756081152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nothin\u0027 like runnin\u0027 into old friends and reminiscin\u0027, huh? Ah, the golden olden days…",
              "maleVariant": "",
              "stringId": "1874958555719979008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not in the mood, Johnny.",
              "maleVariant": "",
              "stringId": "1874961933476876288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Should be. Look at your luck. I mean, coulda wound up all jittery like him.",
              "maleVariant": "",
              "stringId": "1874962511955283968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Have a nice night, V. I\u0027ll be at home.",
              "maleVariant": "",
              "stringId": "1874964066062356480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re not goin\u0027 anywhere!",
              "maleVariant": "",
              "stringId": "1934148483825934336"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
