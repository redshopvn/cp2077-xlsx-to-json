  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. The rest will stay here in camp.",
              "maleVariant": "",
              "stringId": "1776544926192177152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, you\u0027re here.",
              "maleVariant": "",
              "stringId": "1776648188379598848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What is she doing here?",
              "maleVariant": "What is he doing here?",
              "stringId": "1776652853401948160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I forbade you from even breathing on that Militech convoy so you call in your friend?",
              "maleVariant": "",
              "stringId": "1776653112710598656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am starting to wonder.",
              "maleVariant": "",
              "stringId": "1776660594442964992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh. That\u0027s exactly why I had V come and stand right in front of you. Do you think I\u0027m that stupid?",
              "maleVariant": "",
              "stringId": "1776661433958076416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got no notion what\u0027s going on.",
              "maleVariant": "",
              "stringId": "1776664477932261380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0022Forbade\u0022 Panam from doing something?",
              "maleVariant": "",
              "stringId": "1776665135649460224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You really think you can \u0022forbid\u0022 Panam from doing anything?",
              "maleVariant": "",
              "stringId": "1780583596230512640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Trust me – no point. She\u0027ll do whatever she wants.",
              "maleVariant": "",
              "stringId": "1780584580549775360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, she won\u0027t. Not if she plans to stay in this family.",
              "maleVariant": "",
              "stringId": "1780591402333573120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Got no fuckin\u0027 clue what\u0027s going on here.",
              "maleVariant": "",
              "stringId": "1780727836516356096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dammit to hell, Saul!",
              "maleVariant": "",
              "stringId": "1780735371902259200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you seriously considering letting that Basilisk slip through our fingers?",
              "maleVariant": "",
              "stringId": "1780736837392076800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I am.",
              "maleVariant": "",
              "stringId": "1780737680715616256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or pissing Militech off so they hammer us into the dirt right here?",
              "maleVariant": "",
              "stringId": "1780742431167549440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shit, Panam, can you please tell him…?",
              "maleVariant": "",
              "stringId": "1780743047445024768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027ve already tried. Let it go, Mitch.",
              "maleVariant": "",
              "stringId": "1780743552456003584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The Biotechnica people will be here in an hour. Try not to cock anything else up before then.",
              "maleVariant": "",
              "stringId": "1780746055901843456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can\u0027t believe it. There he goes, off to paint the grass green.",
              "maleVariant": "",
              "stringId": "1780747141622599680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027s this about a Basilisk?",
              "maleVariant": "",
              "stringId": "1780747809120915456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bein\u0027 unusually quiet.",
              "maleVariant": "",
              "stringId": "1780749884412203008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bein\u0027 unusually quiet. What\u0027s up?",
              "maleVariant": "",
              "stringId": "1780750486764589056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I see no point in arguing with Saul. We can\u0027t win.",
              "maleVariant": "",
              "stringId": "1780752107661758464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So – givin\u0027 up?",
              "maleVariant": "",
              "stringId": "1780752595878744064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Of course not. We\u0027ll take the Basilisk ourselves, then wait for Saul to admit we were right.",
              "maleVariant": "",
              "stringId": "1780753108120702976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Either that or to boot us out of the family.",
              "maleVariant": "",
              "stringId": "1780753455694286848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s an armored Militech cargo panzer – junk from back in 2060 or so.",
              "maleVariant": "",
              "stringId": "1780796771714535424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They want to peddle it off to a backwards country, somewhere it can still pass as a technological wonder.",
              "maleVariant": "",
              "stringId": "1780804510457073664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The Militech convoy will take the one-oh-one, right past us.",
              "maleVariant": "",
              "stringId": "1780819596445814784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shame not to say hello.",
              "maleVariant": "",
              "stringId": "1780828145242673152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lemme guess – whole thing\u0027s your idea?",
              "maleVariant": "",
              "stringId": "1780832286463561728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How did you know?",
              "maleVariant": "",
              "stringId": "1780837009736404992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul wouldn\u0027t be quite so pissed if it weren\u0027t.",
              "maleVariant": "",
              "stringId": "1780839069408104448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027s the plan?",
              "maleVariant": "",
              "stringId": "1780839744993038340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0027Member when you told me to give you a good kick in the pants?",
              "maleVariant": "",
              "stringId": "1780842766267650048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "On the contrary, easy as pie. There won\u0027t even be much security on the convoy.",
              "maleVariant": "",
              "stringId": "1780843283089788928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Plus, we\u0027ll be doing our part towards furthering the cause of world peace.",
              "maleVariant": "",
              "stringId": "1780844154045403136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought you and Saul were getting along.",
              "maleVariant": "",
              "stringId": "1780844865953009664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I believe V is right. If this had been Saul\u0027s idea, he would\u0027ve seen it as the greatest plan on God\u0027s green earth.",
              "maleVariant": "",
              "stringId": "1780845707632381952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0027Member, a while back, told me to give you a solid kick in the pants if I saw you divin\u0027 in the stupid pool again?",
              "maleVariant": "",
              "stringId": "1780854954730524672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why should this beast wind up being used in some little third world skirmish?",
              "maleVariant": "",
              "stringId": "1780857119494705152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Basilisk worth a fight with Saul?",
              "maleVariant": "",
              "stringId": "1780858936819519488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bull. Saul isn\u0027t that petty.",
              "maleVariant": "",
              "stringId": "1780860496261746688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He\u0027s simply a coward. He would rather play it safe, bide his time. For the family.",
              "maleVariant": "",
              "stringId": "1780887625020018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought the Biotechnica job was a bust.",
              "maleVariant": "",
              "stringId": "1780911523258241028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought the Biotechnica contract had withered and died.",
              "maleVariant": "",
              "stringId": "1780911964784234496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This Basilisk worth another fight with Saul?",
              "maleVariant": "",
              "stringId": "1780930162627776512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I do. Yet that was before our tandem raid on the Raffen camp.",
              "maleVariant": "",
              "stringId": "1780942008365682688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The family is falling apart. If we fall into this Biotechnica rut, no one will ever take us seriously again.",
              "maleVariant": "",
              "stringId": "1782044560914190336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why\u0027s Saul got his briefs in a bunch over this Basilisk?",
              "maleVariant": "",
              "stringId": "1782045603114848256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Saul insists we see it through to the end – to prove the Aldecaldos keep their word.",
              "maleVariant": "",
              "stringId": "1782059656314949632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Crop protection! Who gives a damn about crop protection?!",
              "maleVariant": "",
              "stringId": "1782060507674775552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All Saul is going to prove is that a corp can walk all over us, and we\u0027ll say thank you and ask for more.",
              "maleVariant": "",
              "stringId": "1782065922219249664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, so you do wanna go behind Saul\u0027s back, dragged me here specifically for that.",
              "maleVariant": "",
              "stringId": "1782087362695991296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cassidy, Ted, Carol and Bob – also on board.",
              "maleVariant": "",
              "stringId": "1782115196332216320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He\u0027s afraid Militech will destroy us if we pick a fight with it.",
              "maleVariant": "",
              "stringId": "1782121599423811584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He thinks it safer to stick with the corporations. Like Biotechnica – what a joke!",
              "maleVariant": "",
              "stringId": "1782123551335452672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We would rather go back to plying the trade we know best – logistics.",
              "maleVariant": "",
              "stringId": "1782124547281326080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meanin\u0027 smugglin\u0027.",
              "maleVariant": "",
              "stringId": "1782167584028037120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That\u0027s what I said.",
              "maleVariant": "",
              "stringId": "1782167869794357248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I believe you lost your opportunity there.",
              "maleVariant": "",
              "stringId": "1782169260206133248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nope, different, that – rescue mission. Someone from the family.",
              "maleVariant": "",
              "stringId": "1782171193360846848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now, you\u0027re out to plain trick him.",
              "maleVariant": "",
              "stringId": "1782171699764334592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you plan to lecture me, you can damn well go right now.",
              "maleVariant": "",
              "stringId": "1782172056263397376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Terrible idea, all this. Sorry, can\u0027t, won\u0027t get involved.",
              "maleVariant": "Terrible idea, all this. Sorry, I can\u0027t-- won\u0027t get involved.",
              "stringId": "1782182281473232896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Suit yourself.",
              "maleVariant": "",
              "stringId": "1782183723676274688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you plan to grab the Basilisk how?",
              "maleVariant": "",
              "stringId": "1782184768997494784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But if you change your mind...",
              "maleVariant": "",
              "stringId": "1782187483785924608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck, Mitch. You heard her. She said this is not her concern.",
              "maleVariant": "Fuck, Mitch. You heard him. He said this is not his concern.",
              "stringId": "1782189187512209408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought you and Saul\u0027d agreed to bury your carbines.",
              "maleVariant": "",
              "stringId": "1782192144563638272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks, V.",
              "maleVariant": "",
              "stringId": "1782235166898720768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So did I. But it seems that\u0027s only true when somebody is trying to kill us.",
              "maleVariant": "",
              "stringId": "1782236431900807168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0027Zactly. You saved his life. Wanna fuck things up all over again now?",
              "maleVariant": "Zactly. You saved his life. You wanna fuck things up all over again now?",
              "stringId": "1782237230915719168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, let\u0027s go talk to the rest.",
              "maleVariant": "",
              "stringId": "1782239351438381056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I saved his life, yet somehow he still ignores everything I say.",
              "maleVariant": "",
              "stringId": "1782249340861222912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So I do believe we have come full circle.",
              "maleVariant": "",
              "stringId": "1782251740674199552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I want patrols every half hour, reporting directly to me.",
              "maleVariant": "",
              "stringId": "1782261366148894720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are we preparing for a visit from Biotechnica?",
              "maleVariant": "",
              "stringId": "1782261893591011328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The Raffens could surprise us at any moment.",
              "maleVariant": "",
              "stringId": "1782262347783802880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I don\u0027t want any more surprises.",
              "maleVariant": "",
              "stringId": "1782263412130385920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. OK, everyone listen. Militech will transport the Basilisk in parts distributed across two trucks.",
              "maleVariant": "",
              "stringId": "1791111866189418496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wait – do I sense a quiver in your voice?",
              "maleVariant": "",
              "stringId": "1791119057642393600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Fuck you.",
              "maleVariant": "",
              "stringId": "1791119366477385728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Haven\u0027t said I\u0027d help just yet.",
              "maleVariant": "",
              "stringId": "1791140523922493440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The convoy we\u0027ll halt at the railroad crossing on the one-oh-one.",
              "maleVariant": "",
              "stringId": "1791163498558197760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We\u0027ll attack from behind, then get the hell out and away.",
              "maleVariant": "",
              "stringId": "1791166483560468480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Questions, comments, motions?",
              "maleVariant": "",
              "stringId": "1791168305817784320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, I\u0027m in.",
              "maleVariant": "",
              "stringId": "1791172326645370884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It all seems too perfect, too easy…",
              "maleVariant": "",
              "stringId": "1791173472294342656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t wanna get involved.",
              "maleVariant": "",
              "stringId": "1791174393682268160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam\u0027s got a beef with Saul, I know. But you guys?",
              "maleVariant": "",
              "stringId": "1791179111368298496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK. I\u0027m in.",
              "maleVariant": "",
              "stringId": "1791180345147985920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hmm. OK. Thanks.",
              "maleVariant": "",
              "stringId": "1791180838146478080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s all too... easy. Convenient to arrange.",
              "maleVariant": "",
              "stringId": "1791181282021281792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The exact tech you need. Comin\u0027 your way almost served on a silver platter.",
              "maleVariant": "",
              "stringId": "1791182237936717824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Panam\u0027s got a beef with Saul, I know. But you guys...?",
              "maleVariant": "",
              "stringId": "1791182427989020672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Nice job, sunshine.",
              "maleVariant": "",
              "stringId": "1791193858188840960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. Damn short, sweet and pertinent.",
              "maleVariant": "",
              "stringId": "1791194391754641408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "None of that sad-sack bullshit Saul likes to tack on.",
              "maleVariant": "",
              "stringId": "1791194920975142912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A few years back, nobody would have given corpo work a second thought.",
              "maleVariant": "",
              "stringId": "1791205783987621888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We want a good name, we just need good gear and good jobs.",
              "maleVariant": "",
              "stringId": "1791207949288673280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And Saul would see that if he pulled his head out of his caboose once in a while.",
              "maleVariant": "",
              "stringId": "1791208706092109824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes. Saul also claimed it stinks to high heaven.",
              "maleVariant": "",
              "stringId": "1791213768130498560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Intel on the convoy – where\u0027d you get it?",
              "maleVariant": "",
              "stringId": "1791213925450452992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Carol hacked the Militech comms channel. Is that enough for you?",
              "maleVariant": "",
              "stringId": "1791214380163338240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027s this Basilisk nonsense?",
              "maleVariant": "",
              "stringId": "1802438251616997376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, just the four vets on board with the plan, nobody else?",
              "maleVariant": "",
              "stringId": "1802443723489218564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We didn\u0027t bother asking the others.",
              "maleVariant": "",
              "stringId": "1802443723539550208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "There was too great a risk someone would take it to Saul.",
              "maleVariant": "",
              "stringId": "1802448238724583424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Besides, only the vets understand what we stand to gain by having a Basilisk.",
              "maleVariant": "",
              "stringId": "1802453846743912448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To do it all behind my back, is that it?",
              "maleVariant": "",
              "stringId": "1824359683410169856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And you guys\u0027d rather...?",
              "maleVariant": "",
              "stringId": "1872244887184494592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Did you try to get Saul on board again? No luck, huh?",
              "maleVariant": "",
              "stringId": "1874979166444343296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just the vets on board, nobody else?",
              "maleVariant": "",
              "stringId": "1876487755657576452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Or a Fourth Corporate War?",
              "maleVariant": "",
              "stringId": "1877830541561843712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re not blowing this a little out of proportion...?",
              "maleVariant": "",
              "stringId": "1877831674795675648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Besides, we\u0027ll look good, aware, prepared.",
              "maleVariant": "",
              "stringId": "1877831981567070208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get the sense this fight\u0027s grown stale. C\u0027mon, put some feeling into it.",
              "maleVariant": "",
              "stringId": "1877844127701028864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Feel like this fight\u0027s been goin\u0027 on a while.",
              "maleVariant": "",
              "stringId": "1877846715922141188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wow, a lot\u0027s changed since I left.",
              "maleVariant": "",
              "stringId": "1877847454337748992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But a chance like this...",
              "maleVariant": "",
              "stringId": "1877859196426014720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A chance at what? Rotting up the job we came here to do?",
              "maleVariant": "",
              "stringId": "1877860381249785856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No. This discussion ended long ago.",
              "maleVariant": "",
              "stringId": "1877934062168723456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wow, seems a lot changed while I was away.",
              "maleVariant": "",
              "stringId": "1877948591590100992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "No, nothing has changed at all.",
              "maleVariant": "",
              "stringId": "1877951587698888704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1887983066578092032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lemme guess. Your idea?",
              "maleVariant": "",
              "stringId": "1888135099156549632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes! A craft like that would finally put us back on the right path.",
              "maleVariant": "",
              "stringId": "1888148922475237376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Basilisk – what\u0027s Saul\u0027s problem with it?",
              "maleVariant": "",
              "stringId": "1888150337297862656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds pretty darn risky, this.",
              "maleVariant": "",
              "stringId": "1888159554364788740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds pretty risky to me.",
              "maleVariant": "",
              "stringId": "1888160794972803072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We\u0027ll make better use of it right here in America.",
              "maleVariant": "",
              "stringId": "1888167615112433664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "First, we\u0027ll ride to the station.",
              "maleVariant": "",
              "stringId": "1888174315546628096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The train engine we need to block the convoy\u0027s way is there.",
              "maleVariant": "",
              "stringId": "1888184988389363712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Is everything clear?",
              "maleVariant": "",
              "stringId": "1892182770691362820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I think I heard a little shake in that voice.",
              "maleVariant": "",
              "stringId": "1934231520742756352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But – are you sure you didn\u0027t leave any tracks? Militech won\u0027t notice a thing?",
              "maleVariant": "",
              "stringId": "1934232031055335428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you trying to insult me, Bobby?",
              "maleVariant": "",
              "stringId": "1934236677253648384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You did not.",
              "maleVariant": "",
              "stringId": "1934237810185490432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are we going?",
              "maleVariant": "",
              "stringId": "1934241419333750788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027m riding with.",
              "maleVariant": "",
              "stringId": "1934249007584661508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Are you riding with us, or will we meet you there?",
              "maleVariant": "",
              "stringId": "1934249743315914752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V?",
              "maleVariant": "",
              "stringId": "1934253358050988032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well? V?",
              "maleVariant": "",
              "stringId": "1934254185167736836"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We\u0027ll meet there.",
              "maleVariant": "",
              "stringId": "1934261405880176640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ridin\u0027 with you.",
              "maleVariant": "",
              "stringId": "1934263444446138368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Meet you there.",
              "maleVariant": "",
              "stringId": "1934264412810268676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Excellent. Let\u0027s get going.",
              "maleVariant": "",
              "stringId": "1934267567144980480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not a problem.",
              "maleVariant": "",
              "stringId": "1934267782480547840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let\u0027s go.",
              "maleVariant": "",
              "stringId": "1934289029532766212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit]",
              "maleVariant": "",
              "stringId": "1957711485775314948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come on, let\u0027s go.",
              "maleVariant": "",
              "stringId": "1993677816921190400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Aw, come on. Did your hands get clammy, too?",
              "maleVariant": "",
              "stringId": "1993768991745241088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Screw you.",
              "maleVariant": "",
              "stringId": "1993821569174597632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, OK. Forget I even asked.",
              "maleVariant": "",
              "stringId": "1993825663066959872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you don\u0027t want to, you don\u0027t need to drop into the middle of this.",
              "maleVariant": "",
              "stringId": "2230209024333791232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But we could use your help – that\u0027s undeniable.",
              "maleVariant": "",
              "stringId": "2230211747443720192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in]",
              "maleVariant": "",
              "stringId": "2266579595219980292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, I\u0027m in.",
              "maleVariant": "",
              "stringId": "2429848719008755712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] Don\u0027t wanna get involved.",
              "maleVariant": "",
              "stringId": "2429848719008755736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It all seems too perfect, too easy…",
              "maleVariant": "",
              "stringId": "2429848719008755760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] I\u0027m riding with.",
              "maleVariant": "",
              "stringId": "2429861517239545856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand] We\u0027ll meet there.",
              "maleVariant": "",
              "stringId": "2429861517239545880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get up]",
              "maleVariant": "",
              "stringId": "2472132365350367236"
            }
          ]
        }
      }
    },
