  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, you are here.",
              "maleVariant": "",
              "stringId": "1643116584492875776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027re we doin\u0027 here?",
              "maleVariant": "",
              "stringId": "1643117143157391364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do you know who most wished to honor Arasaka-sama with the parade?",
              "maleVariant": "",
              "stringId": "1643118453508292608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t see what that gets us.",
              "maleVariant": "",
              "stringId": "1643121473541390336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thought Yori wasn\u0027t one for tradition.",
              "maleVariant": "",
              "stringId": "1643122058495803392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027s in it for Yorinobu?",
              "maleVariant": "",
              "stringId": "1643122915090120704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Parade stands to confirm his legitimacy.",
              "maleVariant": "",
              "stringId": "1643123905851834368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yorinobu\u0027s a smart little muffin. Puttin\u0027 on the parade to show off his attachment to Arasaka, its values.",
              "maleVariant": "",
              "stringId": "1643125303024828416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He is shrewd. I will not deny that.",
              "maleVariant": "",
              "stringId": "1643127550299357184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Japanese heritage is one element. Yorinobu will also put on a display of power.",
              "maleVariant": "",
              "stringId": "1643127703458562048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The floats are sure to be beautiful, but the wizened eye will see in them a hidden message to his foes.",
              "maleVariant": "",
              "stringId": "1643128638721245184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Still don\u0027t see how this gets us anywhere.",
              "maleVariant": "",
              "stringId": "1644748867821912064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I acquired some information of my own.",
              "maleVariant": "",
              "stringId": "1698600601962692608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have had an idea.",
              "maleVariant": "",
              "stringId": "1711266717562322944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look to the sky.",
              "maleVariant": "",
              "stringId": "1711563643113455616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "While you were occupied with your shady dealings, I learned more about the floats.",
              "maleVariant": "",
              "stringId": "1711566497521876992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have an infected shard.",
              "maleVariant": "",
              "stringId": "1711566497555431424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Take shard]",
              "maleVariant": "",
              "stringId": "1711566497605763072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And then?",
              "maleVariant": "",
              "stringId": "1711566501028315136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hacking\u0027s not my forte.",
              "maleVariant": "",
              "stringId": "1711566501028315152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right. Then what?",
              "maleVariant": "",
              "stringId": "1711566504215986176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Gotta be honest. Netrunning\u0027s not my forte.",
              "maleVariant": "",
              "stringId": "1711566504249540608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It is even less mine.",
              "maleVariant": "",
              "stringId": "1711566504249540616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "They are all kept in one place – Arasaka Industrial Park. It is there that they prepare them.",
              "maleVariant": "",
              "stringId": "1711569540120330240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Let us move on.",
              "maleVariant": "",
              "stringId": "1711571853195100160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "To take control of the float is one thing, but not all. The security concerns me – the snipers especially. I will be an easy target.",
              "maleVariant": "",
              "stringId": "1711572544248627200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But I may have an answer. Okada-san mentioned the city cameras.",
              "maleVariant": "",
              "stringId": "1711573935180496896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Doesn\u0027t seem like the kind who\u0027d care to honor tradition.",
              "maleVariant": "",
              "stringId": "1714341936410529792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That is so.",
              "maleVariant": "",
              "stringId": "1714343242885894144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Cmothertongue l=\u0022jpn\u0022 m=\u0022gumin-domo\u0022 b=\u0022\\\u0022I am here, \u0022 a=\u0022. Watch your backs.\\\u0022 \u0022/\u003E",
              "maleVariant": "",
              "stringId": "1714347411420090368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What message\u0027d that be?",
              "maleVariant": "",
              "stringId": "1714349377978241024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And to solidify his legitimacy.",
              "maleVariant": "",
              "stringId": "1714354294189068288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Good to see you, Goro.",
              "maleVariant": "",
              "stringId": "1714358578536386560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Goro. Good to see you.",
              "maleVariant": "",
              "stringId": "1714359452428013568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why? What\u0027re we doin\u0027 here?",
              "maleVariant": "",
              "stringId": "1714359614143598592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "As it is to see you, V.",
              "maleVariant": "",
              "stringId": "1714360581282992128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Actually sounds doable.",
              "maleVariant": "",
              "stringId": "1719999005323038720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Lots of ways to skin this particular cat, y\u0027know. Gonna need--",
              "maleVariant": "",
              "stringId": "1719999561370308608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I know what you need. It is taken care of.",
              "maleVariant": "",
              "stringId": "1720009300359647232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It was not cheap, but the man who sold it to me guaranteed it will work.",
              "maleVariant": "",
              "stringId": "1720013574405754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And how ya aim to do that?",
              "maleVariant": "",
              "stringId": "1724371748889956352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "In front of everyone? Think you might\u0027ve lost it.",
              "maleVariant": "",
              "stringId": "1724371748889956368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Impossible. Even with implants.",
              "maleVariant": "",
              "stringId": "1724371748889956384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And how ya aim to manage that?",
              "maleVariant": "",
              "stringId": "1724373624230383616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re gnatshit crazy. In front of a crowd? What about security or...?",
              "maleVariant": "",
              "stringId": "1724373701741121536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Expectation\u0027s way too high, Goro. Couldn\u0027t pull that off even if you had your fancy implants.",
              "maleVariant": "Expectation\u0027s way too high, Goro. Couldn\u0027t pull that off even if you still had your fancy implants.",
              "stringId": "1724373802756739072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We need only to break into the compound, find the right float and inject a virus into its system.",
              "maleVariant": "",
              "stringId": "1724380572011073536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You will then be able to disable any security before I sneak inside.",
              "maleVariant": "",
              "stringId": "1724386493898559488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I have thought of a solution.",
              "maleVariant": "",
              "stringId": "1724388168130498560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You need not worry.",
              "maleVariant": "",
              "stringId": "1724388318437576704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Think I see where this is goin\u0027. Got me doin\u0027 all the dirty work.",
              "maleVariant": "",
              "stringId": "1724405553470246912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Do you wish to trade places? What exactly would you say to her? \u0022Hello, my name is V\u0022?",
              "maleVariant": "",
              "stringId": "1724410364940914688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hanako-sama knows me. Only I can do this.",
              "maleVariant": "",
              "stringId": "1724418373981179904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not one for subtlety, is he?",
              "maleVariant": "",
              "stringId": "1725524809985429504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I do not understand. Does Arasaka not have enough of its own people?",
              "maleVariant": "",
              "stringId": "1731660491478888448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Look around, sir. Things\u0027re heating up in the city.",
              "maleVariant": "",
              "stringId": "1731661366947573760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Not one to butt in, but…",
              "maleVariant": "",
              "stringId": "1732780378098319360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Need to steal your buddy for a second.",
              "maleVariant": "",
              "stringId": "1732781618672779280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hate to interrupt, but...",
              "maleVariant": "",
              "stringId": "1732782147775840256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He\u0027s got the gift of the gab, can\u0027t be denied, but I need to steal your buddy for a sec.",
              "maleVariant": "",
              "stringId": "1732782508787974144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ah, perfect timing!",
              "maleVariant": "",
              "stringId": "1732783915305877504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What are you...?",
              "maleVariant": "",
              "stringId": "1732786405564825600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The dashi floats will pass precisely this way.",
              "maleVariant": "",
              "stringId": "1732801942743236632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If I could just get onto Hanako-sama\u0027s float, I could speak with her in private.",
              "maleVariant": "",
              "stringId": "1732801942743236644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Get there how?",
              "maleVariant": "",
              "stringId": "1763553732555837440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Shard – where\u0027d you get it?",
              "maleVariant": "",
              "stringId": "1763555229637152768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where\u0027d you get this shard?",
              "maleVariant": "",
              "stringId": "1763556256486658048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Across the canal.",
              "maleVariant": "",
              "stringId": "1763556749871026176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Kabuki? Might actually be half-decent soft, then.",
              "maleVariant": "",
              "stringId": "1763560242719625216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And if it isn\u0027t, got a few tricks up my sleeve, too.",
              "maleVariant": "",
              "stringId": "1763563642941214720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "An\u0027 riots\u0027re always best put down by folks not sportin\u0027 your brand name.",
              "maleVariant": "",
              "stringId": "1770473777101934592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Suits know one spark\u0027s all it takes for Japantown to go up in flames.",
              "maleVariant": "",
              "stringId": "1770492434590863360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why, jump, of course.",
              "maleVariant": "",
              "stringId": "1770501671874334720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You shall learn soon.",
              "maleVariant": "",
              "stringId": "1772240340670877696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds like a plan.",
              "maleVariant": "",
              "stringId": "1780946454967095296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Why\u0027s the dirty work my job?",
              "maleVariant": "",
              "stringId": "1780946454967095316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sounds like a plan.",
              "maleVariant": "",
              "stringId": "1780950257556656128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You agree just like that? The risks are considerable.",
              "maleVariant": "",
              "stringId": "1783722654030499840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, well – gettin\u0027 more \u0027n\u0027 more used to that.",
              "maleVariant": "",
              "stringId": "1834426652978712576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "His murderer. The irony makes me sick.",
              "maleVariant": "",
              "stringId": "1871963463881662464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is good to know.",
              "maleVariant": "",
              "stringId": "1996584541047562240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And now... what would you say to a small test? We must try the shard.",
              "maleVariant": "",
              "stringId": "1996671649260257280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The camera control room is in front of us.",
              "maleVariant": "",
              "stringId": "1996728020454359040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You must get inside and infect the network, that is all.",
              "maleVariant": "",
              "stringId": "1996728467550388224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Uh-huh. And in the meantime you\u0027ll do what? Crack open a can o\u0027 suds?",
              "maleVariant": "",
              "stringId": "2005630399353266176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I would not touch alcohol at work. I will watch your back. Now, get to it.",
              "maleVariant": "",
              "stringId": "2005631121343979520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If we gain access to them, we will see exactly where the snipers are. You can deal with them as I advance.",
              "maleVariant": "",
              "stringId": "2005638444749758464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Wait]",
              "maleVariant": "",
              "stringId": "2014108158846320640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2069428144805330948"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}