  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Knock] Hey, Barry!",
              "maleVariant": "",
              "stringId": "1772111680413790212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Barry? Am I rememberin\u0027 that right?",
              "maleVariant": "",
              "stringId": "1772112117208608768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Who is it?",
              "maleVariant": "",
              "stringId": "1772112601784938496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I remember. Whaddaya want?",
              "maleVariant": "",
              "stringId": "1772113695424540672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Stay strong.",
              "maleVariant": "",
              "stringId": "1772115389705252868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People like us have to get used to loss.",
              "maleVariant": "",
              "stringId": "1772116722118193152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I just wanna talk.",
              "maleVariant": "",
              "stringId": "1772117316618842112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just wanna talk, that\u0027s all. I know it\u0027s what you need right now, even if you don\u0027t wanna admit it.",
              "maleVariant": "",
              "stringId": "1772118135397314560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I can\u0027t turn back time for you, but I can sit next to you and listen...  remind you that you\u0027re not in this alone.",
              "maleVariant": "",
              "stringId": "1772118743839830016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Your friends asked me to drop by.",
              "maleVariant": "",
              "stringId": "1772118999457492996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You should get out there, live a little!",
              "maleVariant": "",
              "stringId": "1772120695365595136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Just wanted to see what\u0027s shakin\u0027. Friends say you never leave your unit. Think I\u0027ve got an inkling as to why.",
              "maleVariant": "I just wanted to see what\u0027s shakin\u0027. Friends say you never leave your unit. Think I\u0027ve got an inkling as to why.",
              "stringId": "1772121400058998784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come in.",
              "maleVariant": "",
              "stringId": "1772121758504218624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You can\u0027t let yourself fall apart, Barry. Not here, not in Night City.",
              "maleVariant": "",
              "stringId": "1772122036754345984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Now hold on a sec. We barely know each other, and you just rock up here talkin\u0027 to me about my problems?",
              "maleVariant": "",
              "stringId": "1772219995496017920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, you got me. Your friends asked me to drop in on you, so I thought, hey, why not.",
              "maleVariant": "",
              "stringId": "1772222129339789312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, come on in. You wanna talk, so let\u0027s talk.",
              "maleVariant": "",
              "stringId": "1772226181909868544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell Petrova and Mendez to drop the act already. Mendez especially.",
              "maleVariant": "",
              "stringId": "1772230192000036864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I lost someone close to me, too. Damned good friend.",
              "maleVariant": "I lost someone close to me, too. A damned good friend.",
              "stringId": "1772241123044126720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whaddaya mean \u0022too\u0022? Wait, this about Andrew? They... told you about him.",
              "maleVariant": "",
              "stringId": "1772241740697333760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Best bud I ever had... known him my whole life. Only person I could spill to without bein\u0027 judged.",
              "maleVariant": "",
              "stringId": "1772242131874902016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everyone has their limits.",
              "maleVariant": "",
              "stringId": "1772243795369422848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We have to forget the dead.",
              "maleVariant": "",
              "stringId": "1772244079743234048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Can you tell me how Andrew died?",
              "maleVariant": "",
              "stringId": "1772244463975034880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Does it matter? Normally. From old age. It\u0027s no wonder, seein\u0027 as he was only a few years younger than my grandma.",
              "maleVariant": "",
              "stringId": "1772244910802628608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mendez doesn\u0027t know shit about life. Everyone\u0027s got their limits, even him. He just hasn\u0027t reached \u0027em. Not like you.",
              "maleVariant": "",
              "stringId": "1772245939564736512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What if he\u0027s right though? Maybe my genes are soft? Don\u0027t only the strongest survive?",
              "maleVariant": "",
              "stringId": "1772246354498842624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you felt nothing, that means his loss wouldn\u0027t have had any meaning.",
              "maleVariant": "",
              "stringId": "1772246468885901312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I guess so...",
              "maleVariant": "",
              "stringId": "1772246637194932224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mendez is right. You gotta toughen up. Tell the world to go fuck itself.",
              "maleVariant": "",
              "stringId": "1772247209633542144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Knock] Barry! Everything OK?",
              "maleVariant": "",
              "stringId": "1773738240396914688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hey, Barry? Everything all right in there?",
              "maleVariant": "Hey, Barry! Everything all right in there?",
              "stringId": "1793747692638334976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V, your neighbor from upstairs. Remember me? Talked about preem rides.",
              "maleVariant": "",
              "stringId": "1837130920566177792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You gave me this look... I was about to hightail it to the Badlands right then an\u0027 there.",
              "maleVariant": "",
              "stringId": "1837131091962216448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Back then you were pumped \u0027bout the Mizutani Shion. I said it was for flash-posers.",
              "maleVariant": "",
              "stringId": "1837131550449975296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You\u0027re a badge. Ex-badge, but still. You\u0027ve gotta stay strong... if you won\u0027t, who will?",
              "maleVariant": "You\u0027re a badge. Ex-badge, but still. Gotta stay strong. I mean, if you won\u0027t, who will?",
              "stringId": "1837160190080434176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He\u0027s a stand-up guy. Who cares if he has shitty taste when it comes to street-shredders?",
              "maleVariant": "",
              "stringId": "1837180973292961792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Come back just to get your ass kicked?",
              "maleVariant": "",
              "stringId": "1837181622453780480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Man, you really know how to cheer a guy up. Maybe those two asshats really are worried about me...?",
              "maleVariant": "",
              "stringId": "1837181755329331200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe I don\u0027t wanna solve anything? That cross your mind?",
              "maleVariant": "",
              "stringId": "1837195889110401024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You should get out there more ‚Äì go for a drink with your pals. Life goes on, Barry, and it won\u0027t wait for you to catch up.",
              "maleVariant": "",
              "stringId": "1837196278341812224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Wallowing in loneliness and self-loathing isn\u0027t gonna solve anything.",
              "maleVariant": "",
              "stringId": "1837196537096814592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well... all right then. Come on in. Not like you\u0027ll change anythin\u0027.",
              "maleVariant": "",
              "stringId": "1837197199327084544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You never know.",
              "maleVariant": "Hey, you never know.",
              "stringId": "1837200781766017024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh well, I tried.",
              "maleVariant": "Welp, I tried.",
              "stringId": "1837204000844005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027ve seen that look before. It\u0027s all too common \u0027mong people like us.",
              "maleVariant": "",
              "stringId": "1841510432347242496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u0022Us\u0022? Heh, you don\u0027t look like a cop.",
              "maleVariant": "",
              "stringId": "1841510576396419072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Badges don\u0027t get a monopoly on loss. You see death every day in my line of work.",
              "maleVariant": "",
              "stringId": "1841510674224365568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You get used to it, of course... until it hits someone you\u0027re close to.",
              "maleVariant": "",
              "stringId": "1841510817283686400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh...",
              "maleVariant": "",
              "stringId": "1841511057533419520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You, too?",
              "maleVariant": "",
              "stringId": "1841511180376195072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dyin\u0027 of old age? In Night City? That merits a monument. Whole world should gaze up at it in awe.",
              "maleVariant": "",
              "stringId": "1841603088951787520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hehehe... Ashbox in a niche\u0027ll have to do.",
              "maleVariant": "",
              "stringId": "1841603265448099840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right... so I\u0027m just supposed to forget everything I\u0027ve seen? That kid... even Andrew?",
              "maleVariant": "",
              "stringId": "1841696313347653632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He\u0027s gone, and you\u0027re here. It\u0027s time to stop livin\u0027 in the past.",
              "maleVariant": "",
              "stringId": "1841696432616882176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You won\u0027t change anything by sittin\u0027 here alone and wallowin\u0027 in misery.",
              "maleVariant": "You won\u0027t change anything by sittin\u0027 here alone and wallowing in misery.",
              "stringId": "1841696569938395136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I think we\u0027re done here.",
              "maleVariant": "",
              "stringId": "1841696676909924352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Suit yourself.",
              "maleVariant": "",
              "stringId": "1841696866844786688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027m gonna take that as a yes!",
              "maleVariant": "",
              "stringId": "1842890146919989248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Eh, try again later...",
              "maleVariant": "",
              "stringId": "1842890690468233216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Heh, you don\u0027t forget a gonk thing like that.",
              "maleVariant": "",
              "stringId": "1895218491762130944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where\u0027d you get the idea somethin\u0027 with me was up? You watchin\u0027 me? Someone send you?",
              "maleVariant": "",
              "stringId": "1895290963312230400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "None o\u0027 you get it, OK? None o\u0027 you!",
              "maleVariant": "",
              "stringId": "1895316828007940096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What about your friends on the force?",
              "maleVariant": "",
              "stringId": "1895381606264979460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPD hasn\u0027t improved, has it?",
              "maleVariant": "",
              "stringId": "1895381672568537088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guessin\u0027 things at the NCPD haven\u0027t changed a lick, huh?",
              "maleVariant": "",
              "stringId": "1895381798296993792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "One time... a Maelstrom ganger killed a young kid right in front of my eyes, for shits \u0027n\u0027 giggles.",
              "maleVariant": "",
              "stringId": "1895382011434745856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I couldn\u0027t do shit. The son of a bitch had corpo protection.",
              "maleVariant": "",
              "stringId": "1895382153453879296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ehh, typical.",
              "maleVariant": "",
              "stringId": "1895382273578745856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Our chief greases his palms with an anonymous donation and guess what. Officially? Kid blew his own brains out.",
              "maleVariant": "",
              "stringId": "1895382362850312192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Somethin\u0027 died in me then. I couldn\u0027t do it anymore... few months later Andrew passed and everything went to shit.",
              "maleVariant": "",
              "stringId": "1895382503074283520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You didn\u0027t have anyone to unload on at work? How about Petrova and Mendez?",
              "maleVariant": "",
              "stringId": "1895383914172366848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Petrova\u0027s a decent gal, but she\u0027s not good with this stuff. Mendez just doesn\u0027t get it.",
              "maleVariant": "",
              "stringId": "1895403036356624384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He thinks us blues need to be tough. Can\u0027t bear the sight of a kid gettin\u0027 murdered? Born with pussy genes, \u0027ccordin\u0027 to him.",
              "maleVariant": "",
              "stringId": "1895403137036697600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But you told him about Andrew.",
              "maleVariant": "",
              "stringId": "1895403304540422144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Honestly? I thought about it a lot. Anyway... they don\u0027t know everything. Better that way.",
              "maleVariant": "",
              "stringId": "1895403562372677632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How\u0027d Andrew die?",
              "maleVariant": "",
              "stringId": "1895406270953537536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Thanks for the talk, V. I, um... I need time to take all this in.",
              "maleVariant": "",
              "stringId": "1895441300924907520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Sure thing. Take care.",
              "maleVariant": "",
              "stringId": "1895441419825037312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Barry! You there?",
              "maleVariant": "Barry? You there?",
              "stringId": "1905181149760307200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Maybe he\u0027s talkin\u0027 to Mendez...",
              "maleVariant": "",
              "stringId": "1905184717938606080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Right in front of your eyes.",
              "maleVariant": "",
              "stringId": "1905551821829292032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Everyone\u0027s got their limits.",
              "maleVariant": "",
              "stringId": "1967849004892667908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "People die. You move on.",
              "maleVariant": "",
              "stringId": "1967849129346056192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So Andrew was like a grandpa to you?",
              "maleVariant": "",
              "stringId": "1967849460209532928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So then Andrew was more like a grandpa to you than a friend?",
              "maleVariant": "",
              "stringId": "1967849848669192192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Hm. Wouldn\u0027t go that far. He was like a... egh, I don\u0027t know... A window into the past, or somethin\u0027.",
              "maleVariant": "",
              "stringId": "1967850945345470464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He reminded me of my gram-grams, about our little talks... time when everything had its proper place, y\u0027know?",
              "maleVariant": "",
              "stringId": "1967851060621721600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "He was the last... living record of those times.",
              "maleVariant": "",
              "stringId": "1967851178498441216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Sit] I lost someone too.",
              "maleVariant": "",
              "stringId": "2088021792150294532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2088026239135944708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Stand]",
              "maleVariant": "",
              "stringId": "2088026347114106884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I\u0027ll take that as a yes.",
              "maleVariant": "",
              "stringId": "2119785631145299972"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}