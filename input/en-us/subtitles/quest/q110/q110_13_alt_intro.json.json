    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "It\u0027s... it\u0027s all so alive. So real.",
              "maleVariant": "",
              "stringId": "1695167131789840384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Cyberspace moves, breathes at all times.",
              "maleVariant": "",
              "stringId": "1698455700167266304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "De interface will adjust de ebb and flow of data to de capabilities of your brain.",
              "maleVariant": "",
              "stringId": "1698456372195430400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But you... you are an exceptional case.",
              "maleVariant": "",
              "stringId": "1698458056896352256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So – what now?",
              "maleVariant": "",
              "stringId": "1698461584406679552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We can begin. All is ready.",
              "maleVariant": "",
              "stringId": "1698463147875438592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We enhanced your link to Silverhand\u0027s neural network for a short time. To grab de fragment.",
              "maleVariant": "",
              "stringId": "1698465897224933376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We must find de data on Alt. Alt alone.",
              "maleVariant": "",
              "stringId": "1698468660935725056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Dis should only take...",
              "maleVariant": "",
              "stringId": "1698476514971291648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yes. We extract de necessary fragment of Silverhand\u0027s engram.",
              "maleVariant": "",
              "stringId": "1698498645444595712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We are ready now to make contact with Alt.",
              "maleVariant": "",
              "stringId": "1698500107344089088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "So, you don\u0027t actually know what\u0027s on the other side.",
              "maleVariant": "",
              "stringId": "1698504043446734848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "I held up my end of our deal. Your turn.",
              "maleVariant": "",
              "stringId": "1698521865128316928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Before you pass through that wall, I want Johnny\u0027s psyche removed.",
              "maleVariant": "",
              "stringId": "1698524569649090560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We will not pass through.",
              "maleVariant": "",
              "stringId": "1698526104495919104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We take you into de cyberspace. No better place for you to interact with de construct.",
              "maleVariant": "",
              "stringId": "1710203183566229504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell me why you want Johnny first.",
              "maleVariant": "",
              "stringId": "1710204274185932800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How do I know I\u0027ll come back?",
              "maleVariant": "",
              "stringId": "1710205290918776832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027s your plan to contact Alt?",
              "maleVariant": "",
              "stringId": "1710207009811664896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How do I know I\u0027ll come back? Do I get some guarantee?",
              "maleVariant": "",
              "stringId": "1710208961991741440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You will be inside our data fortress, under watch.",
              "maleVariant": "",
              "stringId": "1710217524378144768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If you knew what is out there in de cyberspace, you would not be so choosy.",
              "maleVariant": "",
              "stringId": "1710221128845561856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Under YOUR watch.",
              "maleVariant": "",
              "stringId": "1710222880621793280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How\u0027s this any different from regular netrunning?",
              "maleVariant": "",
              "stringId": "1710228160076124160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How\u0027s all this any different from plain old netrunning?",
              "maleVariant": "",
              "stringId": "1710230618508693504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "De interface show you de data layers direct from de Net. Readable, of course.",
              "maleVariant": "",
              "stringId": "1710230618508693516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What de basic \u0027runners see is only a map. They cannot see beneath de surface. Their tech, it is too weak.",
              "maleVariant": "",
              "stringId": "1710230618525470728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We try to cut out a unique piece of Silverhand\u0027s engram from de biochip.",
              "maleVariant": "",
              "stringId": "1710232243432394752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Alt will know it. If something of de human is left after years beyond de Blackwall, she will answer.",
              "maleVariant": "",
              "stringId": "1710236211378196480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny? Looks like you\u0027re the bait. You OK with that?",
              "maleVariant": "",
              "stringId": "1710237982498557952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Yeah, whatever. Just do what she says.",
              "maleVariant": "",
              "stringId": "1710239780093366272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What do you want from Alt?",
              "maleVariant": "",
              "stringId": "1710241058919878656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What exactly do you want from Alt? All of you?",
              "maleVariant": "",
              "stringId": "1710258908199981056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When de Blackwall was built, everyone think de ICE would be enough, keep de wild AIs out, away.",
              "maleVariant": "",
              "stringId": "1710260073914503168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is our BBS. Data fortress.",
              "maleVariant": "",
              "stringId": "1710262772647137280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Bridge to de deep Net.",
              "maleVariant": "",
              "stringId": "1710263956044845056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beyond, dere are no borders.",
              "maleVariant": "",
              "stringId": "1710265920874942464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Beyond de Blackwall.",
              "maleVariant": "",
              "stringId": "1710266412363485184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What\u0027s the Blackwall, really?",
              "maleVariant": "",
              "stringId": "1710272156311818240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The Blackwall – what is it, exactly? I mean, I know…",
              "maleVariant": "The Blackwall – what is it, exactly? I mean, I know...",
              "stringId": "1710272501117161472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You know only what de NetWatch tell you.",
              "maleVariant": "",
              "stringId": "1710272501117161484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "De final bastion against de postwar AIs. De great victory of humanity over chaos.",
              "maleVariant": "",
              "stringId": "1710272501117161496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But de Blackwall is an AI itself. De boundary condition let no thing pass through, either way.",
              "maleVariant": "",
              "stringId": "1710272501133938712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "If de AIs thought like people, dey would call it a traitor.",
              "maleVariant": "",
              "stringId": "1710298337761366016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "That would be your Saturday morning NetWatch propaganda.",
              "maleVariant": "",
              "stringId": "1710299612158693376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "But technology advances, it cannot be stopped. And now, catastrophe come again. This is sure. And soon.",
              "maleVariant": "",
              "stringId": "1710302965689290752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Alt de only one can give us safe passage past de Blackwall.",
              "maleVariant": "",
              "stringId": "1710308714486239232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The change?",
              "maleVariant": "",
              "stringId": "1710309208692690944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "When de change come, we wish to be on de winning side.",
              "maleVariant": "",
              "stringId": "1710312539423051776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "And none have yet returned.",
              "maleVariant": "",
              "stringId": "1710318153465401344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Few have gone through to de other side.",
              "maleVariant": "",
              "stringId": "1710319172849045504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "She will be de first.",
              "maleVariant": "",
              "stringId": "1710319765554532352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "De corporations, dey do not see de danger. But we – we will be ready.",
              "maleVariant": "",
              "stringId": "1711432609935200256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t tell me I gotta get in that.",
              "maleVariant": "",
              "stringId": "1711450250540290052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh, come on. Not the tub.",
              "maleVariant": "",
              "stringId": "1711453696647565312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We must prepare you for de dive into cyberspace.",
              "maleVariant": "",
              "stringId": "1711457799851282432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "You are not accustomed to de neural demand.",
              "maleVariant": "",
              "stringId": "1711465607598510080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Don\u0027t try an\u0027 get outta this, V. Just do what she says. I\u0027m not about to sizzle with you.",
              "maleVariant": "",
              "stringId": "1711466435956772864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Ooooh...",
              "maleVariant": "",
              "stringId": "1711482561461817344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Vire kouran.\u0022 t=\u0022Turn on the power.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1712793838918373376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Before you cross the Blackwall, remove the construct.",
              "maleVariant": "",
              "stringId": "1712929946431983616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Tout bagay pare. Nou ka kòmanse.\u0022 t=\u0022Everything is ready. We can begin.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1744394999535276032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Looks like you\u0027re the bait.",
              "maleVariant": "",
              "stringId": "1744601818568364036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Here.",
              "maleVariant": "",
              "stringId": "1744726950779588608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Bon.\u0022 t=\u0022Good.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1745841678133301248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Li pare.\u0022 t=\u0022She\u0027s ready.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Li pare.\u0022 t=\u0022 He\u0027s ready.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "stringId": "1745849880077996032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[Get in] I\u0027m ready.",
              "maleVariant": "",
              "stringId": "1751949491834261504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oof... let\u0027s do this.",
              "maleVariant": "Hoo... let\u0027s do this.",
              "stringId": "1751952783792476160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where are we?",
              "maleVariant": "",
              "stringId": "1754712582155177988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, wha… what now?",
              "maleVariant": "OK, wh-what now?",
              "stringId": "1761880934798659584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, what now?",
              "maleVariant": "",
              "stringId": "1761887312422440964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny?",
              "maleVariant": "",
              "stringId": "1761895032894590980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "J-Johnny?",
              "maleVariant": "",
              "stringId": "1761895789412814848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is it? Seriously?",
              "maleVariant": "",
              "stringId": "1766150373731799040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "This is it? Seriously?",
              "maleVariant": "",
              "stringId": "1766150894630801408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Where are we?",
              "maleVariant": "",
              "stringId": "1766151244821630976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Pare plonje.\u0022 t=\u0022Ready to plunge.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1770694349373640704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Ki moun ki pral pwoteje li?\u0022 t=\u0022Who will protect her?\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Ki moun ki pral pwoteje li?\u0022 t=\u0022Who will protect him?\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "stringId": "1770696815926382592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Leon avec Jean-ki-ri.\u0022 t=\u0022Leon and Jean-ki-ri.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1770707973932650496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Direksyon konekte.\u0022 t=\u0022Linking flows.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1770712812934729728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "\u003Ckiroshi l=\u0022creo\u0022 o=\u0022Koneksyon estab.\u0022 t=\u0022Stable connection.\u0022 b=\u0022\u0022 a=\u0022\u0022/\u003E",
              "maleVariant": "",
              "stringId": "1770718902963474432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Must have your own version of a SynthTech interface.",
              "maleVariant": "",
              "stringId": "1792268198003150848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK, hang on. Wanna know what you need Silverhand for first.",
              "maleVariant": "OK, hang on. I wanna know what you need Silverhand for first.",
              "stringId": "1947384246858174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We wish to contact Alt Cunningham. We know she and Silverhand were close.",
              "maleVariant": "",
              "stringId": "1947388088622534656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Alt?",
              "maleVariant": "",
              "stringId": "1947388315484049408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "The Relic which Arasaka held close, it is our only lead.",
              "maleVariant": "",
              "stringId": "1947389177849729024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Alt Cunningham… heard of \u0027er.",
              "maleVariant": "",
              "stringId": "1947390385842835456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Oh? Where, from who?",
              "maleVariant": "",
              "stringId": "1947390613627097088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Alt – how do we contact her?",
              "maleVariant": "",
              "stringId": "1947392754903175172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny told me about her.",
              "maleVariant": "",
              "stringId": "1947396937211465728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "From Johnny. Though granted, not much.",
              "maleVariant": "",
              "stringId": "1947403995973439488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny?",
              "maleVariant": "",
              "stringId": "1947404777103839232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "All right, V. Guess you better tell \u0027er.",
              "maleVariant": "",
              "stringId": "1947406234456707084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell her what?",
              "maleVariant": "",
              "stringId": "1947406234456707100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "About us.",
              "maleVariant": "",
              "stringId": "1947406234473484288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "How we contacting Alt?",
              "maleVariant": "",
              "stringId": "1947406900193415172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Johnny\u0027s construct is active.",
              "maleVariant": "",
              "stringId": "1947407260115030016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Construct\u0027s active. Lets me and Johnny shoot the shit every now and then.",
              "maleVariant": "",
              "stringId": "1947407790761594880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Whoa, what? Said nothin\u0027 \u0027bout breakin\u0027 through the Blackwall.",
              "maleVariant": "Whoa, what? You said nothin\u0027 \u0027bout breakin\u0027 through the Blackwall.",
              "stringId": "1947409036872536064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Well, now you know.",
              "maleVariant": "",
              "stringId": "1947409476016164864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "What, you think I\u0027m hallucinating? Found myself an imaginary friend?",
              "maleVariant": "What, think I\u0027m hallucinating? Found myself an imaginary friend?",
              "stringId": "1947487816991395840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Mhm. We will look at dis, too.",
              "maleVariant": "",
              "stringId": "1947491792621162496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Little is known of how an engram might exist within a human\u0027s neural network.",
              "maleVariant": "",
              "stringId": "1947494987271524352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Tell ya one thing – doesn\u0027t shake out well for the human.",
              "maleVariant": "",
              "stringId": "1947496159713710080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Guess all this means you got your own SynthTech interface.",
              "maleVariant": "",
              "stringId": "1948740867425325056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "We have our own versions of all. We do not use anything from de city.",
